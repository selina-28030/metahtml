function ss(){
   var keyword=document.getElementById('all').value;
   var pattern = /[\/"@#\$%\^&\.',"<>]*/;
   if((solecism = pattern.exec(keyword))!= ""){
      alert("不能包含特殊字符!");
      return;
   }
   
   if(keyword!="")
   {
   		document.getElementById('searchword').value=keyword;
   }
   return true;
 }
</SCRIPT>
