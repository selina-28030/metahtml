            <SCRIPT type="text/javascript">
                function sss() {
                    var keyword = document.getElementById('alls').value;
                    var pattern = /[\/"@#\$%\^&\.',"<>]*/;
                    if ((solecism = pattern.exec(keyword)) != "") {
                        alert("不能包含特殊字符!");
                        return;
                    }

                    if (keyword != "") {
                        document.getElementById('searchwords').value = keyword;
                    }
                    return true;
                }
            </SCRIPT>

