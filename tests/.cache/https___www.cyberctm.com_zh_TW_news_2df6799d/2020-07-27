    <script>
        $(function () {

            var topOffset = {
                val: -54
            };

            var detectScreen = function (){
                var viewportWidth = $(window).width();

                if(viewportWidth < 992){
                    topOffset.val = -124;
                }else{
                    topOffset.val = -54;
                }
            }
            detectScreen();


            $.scrollIt({
                upKey: 38,             // key code to navigate to the next section
                downKey: 40,           // key code to navigate to the previous section
                easing: 'linear',      // the easing function for animation
                scrollTime: 600,       // how long (in ms) the animation takes
                activeClass: 'active', // class given to the active nav element
                onPageChange: null,    // function(pageIndex) that is called when page is changed
                topOffset: topOffset   // offset (in px) for fixed top navigation
            });

            $( window ).resize(function() {
                detectScreen();
            });
        });


    </script>
    <script>
        $(function() {

            var scale = 0;

            $("#bigger-font").click(function(){
                if(scale == 2){
                    alert("已達到最大尺寸！");
                }else{
                    scale++;

                    var originSize = parseInt($('html').css("font-size"));
                    originSize = originSize + 1;
                    $('html').css("font-size",originSize);

                    $('.dotdotdot').trigger("update.dot");

                }
            });

            $("#smaller-font").click(function(){
                if(scale == -2){
                    alert("已為最小尺寸！");
                }else{
                    scale--;

                    var originSize = parseInt($('html').css("font-size"));
                    originSize = originSize - 1;
                    $('html').css("font-size",originSize);

                    $('.dotdotdot').trigger("update.dot");

                }
            });
        });


    </script>
    <script>

        adsUrl = '//ads.cyberctm.com';
        adsIntervalInMin = 60;

        window.onmessage = function(e){
            var message = JSON.parse(e.data);

            if(message.action != undefined){
                switch (message.action){
                    case 'add':
                        if(message.zoneId != undefined){
                            showBigAds(message.zoneId);
                        }
                        break;
                    case 'close':
                        closeAds();
                        break;
                    case 'countdown':
                        adsCountdown();
                        break;
                }
            }
        };

        function closeAds(){
            $(".big-ads").slideUp(1200, function () {});
            $(".big-ads iframe").remove();
        }

        function showBigAds(zoneId){

            //prepend iframe

            iframes = $(".big-ads").find('iframe');
            for (var i = 0; i < iframes.length; i++) {
                iframes[i].parentNode.removeChild(iframes[i]);
            }

            $(".big-ads").prepend('<iframe id="iframe" width="100%" height="100%" scrolling="no" src="' + adsUrl + '/www/cyberctm/iframe2.php?zone='+ zoneId +'" frameborder="0" allowtransparency="true"></iframe>');

            $(".big-ads").slideDown(1200, function () {
            });
        }

        function adsCountdown(){

            setTimeout(function () {
                closeAds();
            }, 6000);//6秒

            /*
             * big ads vanished in 5s
             * */
            var ads;
            var time = 5;
            var adsCounterDiv = document.getElementById("big-ads-sec");
            var adsCounterWrapper = document.getElementById("big-ads-txt");

            if (adsCounterDiv) {
                adsCounterWrapper.style.display = 'block';
                adsCounterDiv.innerHTML = "5";
                adsCounter = setInterval(function () {
                    adsCounterDiv.innerHTML = time.toString();
                    if (time == 0) {
                        window.clearInterval(adsCounter);
                        adsCounterWrapper.style.display = 'none';
                    }
                    time--;
                }, 1000);
            }
        }

        $(function() {

            var time = Cookies.get('B-AD1');

            if (time == undefined){
                time = new Date();
                time = time.getTime();

                InitBigAds();
                Cookies.set('B-AD1', time);


            }else{

                var now = new Date();
                now = now.getTime();
                var timediff = now - time;

                if(timediff/(60*1000) > adsIntervalInMin ){

                    InitBigAds();
                    Cookies.set('B-AD1', now);
                }
            }
        });

        function InitBigAds(){
            var paramObj = {
                action: 'is_zone_active',
                zone: ['NB-AD1-1']
            };
            param = $.param( paramObj );
            param = decodeURIComponent( param );

            url = adsUrl + '/www/cyberctm/CTMAdsAPI.php?' + param;

            $.ajax({
                url: url,
                dataType: 'json'
            }).done(function(data){

                candidate = [];

                for (i = 0; i < paramObj.zone.length; i++) {
                    if(data[paramObj.zone[i]] == 1 ){
                        candidate.push(paramObj.zone[i]);
                    }
                }

                if(candidate.length > 0){
                    shuffle(candidate);
                    showBigAds(candidate[0]);
                }

            });

        }

        function shuffle(array) {
            var currentIndex = array.length, temporaryValue, randomIndex;

            // While there remain elements to shuffle...
            while (0 !== currentIndex) {

                // Pick a remaining element...
                randomIndex = Math.floor(Math.random() * currentIndex);
                currentIndex -= 1;

                // And swap it with the current element.
                temporaryValue = array[currentIndex];
                array[currentIndex] = array[randomIndex];
                array[randomIndex] = temporaryValue;
            }

            return array;
        }

    </script>

    

