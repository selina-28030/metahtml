    <script>
        $(function() {
            var main_nav = new Swiper('div#main-nav', {
                slidesPerView: 'auto',
                roundLengths: true,
                grabCursor: true,
                onSwiperCreated: function(swiper){
                    var weight_wrapper = $('#main-nav').width();
                    var weight_ul = $('div#main-nav .swiper-wrapper').width();
                    if(weight_ul <= weight_wrapper){
                        $('#wrapper-btn-right').hide();
                    }
                    $('div#main-nav .prev-btn-wrapper').hide();
                },
            });

            function main_callbacks(){
                var weight_wrapper = $('.main-nav-wrapper').width();
                var weight_ul = $('div#main-nav .swiper-wrapper').width();
                var x = main_nav.getWrapperTranslate('x');

                if(x >= 0){
                    $('div#main-nav .prev-btn-wrapper').hide();
                }else{
                    $('div#main-nav .prev-btn-wrapper').show();
                }

                if(x <= weight_wrapper-weight_ul){
                    $('div#main-nav .next-btn-wrapper').hide();
                }else{
                    $('div#main-nav .next-btn-wrapper').show();
                }
            }
            main_nav.wrapperTransitionEnd(main_callbacks,true);

            var top_slider = new Swiper('div#top-slider', {
                loop: true,
                autoplay: 5000,
                autoplayDisableOnInteraction : false,
                grabCursor: true,
                pagination: '.pagination',
                paginationClickable :true
            });

            $('div#top-slider .left-btn').click(function () {
                top_slider.swipePrev();
            });
            $('div#top-slider .right-btn').click(function () {
                top_slider.swipeNext();
            });
            $('div#main-nav .prev-btn').click(function () {
                console.log("prev");
                main_nav.swipePrev();
            });
            $('div#main-nav .next-btn').click(function () {
                console.log("next");
                main_nav.swipeNext();
            });
        });
    </script>
