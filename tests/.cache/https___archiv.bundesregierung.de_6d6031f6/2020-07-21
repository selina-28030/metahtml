if (match != null) {
var host = match[3];
var file = match[6];
if (host && file) {
host = host.toLocaleLowerCase().replace("www.","");
_paq.push(['trackEvent', 'Download__'+host, file, downloadURL]);
}
}
}
}
return trackingString;
}]);
