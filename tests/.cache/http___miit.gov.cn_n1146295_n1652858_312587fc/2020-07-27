 function createUrl(pName){ 
   var u=window.location.href; 
  var index=u.indexOf("/manageweb") 
  if(index!=-1){ 
   var uu=purl+"&pageName="+pName 
      return "javascript:pageName="+pName+";goPub('"+uu+"')" 
