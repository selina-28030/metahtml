	<article class="message message--post js-post js-inlineModContainer  "
		data-author="Funky"
		data-content="post-8086"
		id="js-post-8086">

		<span class="u-anchorTarget" id="post-8086"></span>

		<div class="message-inner">
			<div class="message-cell message-cell--user">
				

	<section itemscope itemtype="https://schema.org/Person" class="message-user">
		<div class="message-avatar ">
			<div class="message-avatar-wrapper">
				<a href="/members/funky.125/" class="avatar avatar--m avatar--default avatar--default--dynamic" data-user-id="125" data-xf-init="member-tooltip" style="background-color: #cc6699; color: #602040">
