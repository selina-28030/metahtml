			showSlides();

			function showSlides() {
				var i;
				var slides = document.getElementsByClassName("edge-native-slides-large");
				for (i = 0; i < slides.length; i++) {
					slides[i].style.display = "none";
				}
				slideIndex++;
				if (slideIndex > slides.length) {slideIndex = 1}    
					slides[slideIndex-1].style.display = "block";


				var currentCatText = $(".speaker-cat-anim-text").eq(slideIndex-1);
				currentCatText.toggleClass("animate-in");
				if(lastCatText){
					lastCatText.toggleClass("animate-in");
				}
				lastCatText = currentCatText;
				
