                                   var clickOdd = true
                                    $('.search-tit .upDown').on("click",function(){
                                        var showInput =  $('.search-tit input');
                                        var siteId =  $('#siteId');
                                        if(clickOdd){
                                            showInput.val('±¾Õ¾');
                                            siteId.val('www.shanghai.gov.cn');
                                            clickOdd = false
                                        }else{
                                            showInput.val('Õ¾Èº');
                                            siteId.val('all');
                                            clickOdd = true
