          <li class="data-item">
              <div class="title"> Bình luận/Views</div>
              <div class="content">
                  <%= commentPerView %>
                <span  data-toggle="tooltip" title="<%= commentView.tips %>" class="rate-wrap <%= commentView.className %>">
                  <span class="kolicon kol-icon-<%= commentView.className %>"></span>
                  <span class="rate-text"><%= commentView.text %></span>
                </span>
              </div>
          </li>
          <li class="data-item">
