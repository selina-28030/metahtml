<script type="text/javascript" src="/js/jquery.event.drag-1.5.min.js"></script>
<script type="text/javascript" src="/js/jquery.touchSlider.js"></script>
<script type="text/javascript">
    $(document).ready(function () {
        $(".main_visual").hover(function () {
            $("#btn_prev,#btn_next").fadeIn()
        }, function () {
            $("#btn_prev,#btn_next").fadeOut()
        })
        $dragBln = false;
        $(".main_image").touchSlider({
            flexible: true,
            speed: 200,
            btn_prev: $("#btn_prev"),
            btn_next: $("#btn_next"),
            paging: $(".flicking_con a"),
            counter: function (e) {
                $(".flicking_con a").removeClass("on").eq(e.current - 1).addClass("on");
            }
        });
        $(".main_image").bind("mousedown", function () {
            $dragBln = false;
        })
        $(".main_image").bind("dragstart", function () {
            $dragBln = true;
        })
        $(".main_image a").click(function () {
            if ($dragBln) {
                return false;
            }
        })
        timer = setInterval(function () { $("#btn_next").click(); }, 5000);
        $(".main_visual").hover(function () {
            clearInterval(timer);
        }, function () {
            timer = setInterval(function () { $("#btn_next").click(); }, 5000);
        })
        $(".main_image").bind("touchstart", function () {
            clearInterval(timer);
        }).bind("touchend", function () {
            timer = setInterval(function () { $("#btn_next").click(); }, 5000);
        })
    });
</script>
<script type="text/javascript">
    $(function () {
        $("#news_listl li:eq(0)").show();
        $("#news_listl li:eq(1)").show();
        $("#news_listr li:eq(0)").show();
        $("#news_listr li:eq(1)").show();
        var count = $("#countid").val();
        if (count % 2 == 1)
            count = count / 2 + 1;
        else
            count = count / 2;
        var count2 = $("#countid2").val();
        if (count2 % 2 == 1)
            count2 = count2 / 2 + 1;
        else
            count2 = count2 / 2;
        if(count<8)
        {           
            $("#news_box_btl a:eq(8)").hide();
            if (count < 7) {
                $("#news_box_btl a:eq(7)").hide();
                if (count < 6) {
                    $("#news_box_btl a:eq(6)").hide();
                    if (count < 5) {
                        $("#news_box_btl a:eq(5)").hide();
                        if (count < 4) {
                            $("#news_box_btl a:eq(4)").hide();
                            if (count < 3) {
                                $("#news_box_btl a:eq(3)").hide();
                                if (count < 2) {
                                    $("#news_box_btl a:eq(2)").hide();
                                    if (count < 2) {
                                        $("#news_box_btl a:eq(2)").hide();
                                        if (count < 1) {
                                            $("#news_box_btl a:eq(1)").hide();

                                        }
                                    }
                                }
                            }
                        }
                    }
                }
            }
        }
      
        if (count2 < 8) {
            $("#news_box_btr a:eq(8)").hide();
            if (count2 < 7) {
                $("#news_box_btr a:eq(7)").hide();
                if (count2 < 6) {
                    $("#news_box_btr a:eq(6)").hide();
                    if (count2 < 5) {
                        $("#news_box_btr a:eq(5)").hide();
                        if (count2 < 4) {
                            $("#news_box_btr a:eq(4)").hide();
                            if (count2 < 3) {
                                $("#news_box_btr a:eq(3)").hide();
                                if (count2 < 2) {
                                    $("#.news_box_btr a:eq(2)").hide();
                                    if (count2 < 2) {
                                        $("#news_box_btr a:eq(2)").hide();
                                        if (count2 < 1) {
                                            $("#news_box_btr a:eq(1)").hide();

                                        }
                                    }
                                }
                            }
                        }
                    }
                }
            }
        }
        $("#news_box_btl a").each(function (i) {
            $(this).mouseover(function () {               
                $(this).addClass("cur").siblings().removeClass("cur");
                if (i != 0) {
                    $("#news_listl li").hide();
                    $("#news_listl li:eq(" + (i - 1) * 2 + ")").show();
                    $("#news_listl li:eq(" + ((i - 1) * 2 + 1) + ")").show();
                }
            })          
        })
        $("#news_box_btr a").each(function (i) {
            $(this).mouseover(function () {
                $(this).addClass("cur").siblings().removeClass("cur");
                if (i != 0) {
                    $("#news_listr li").hide();
                    $("#news_listr li:eq(" + (i - 1) * 2 + ")").show();
                    $("#news_listr li:eq(" + ((i - 1) * 2 + 1) + ")").show();
                }
            })
        })
    })
</script>
