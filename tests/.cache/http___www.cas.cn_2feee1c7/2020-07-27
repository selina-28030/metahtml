function isValid(str){
  if(str.indexOf('&') != -1 || str.indexOf('<') != -1 || str.indexOf('>') != -1 || str.indexOf("'") != -1
		|| str.indexOf('\\') != -1 || str.indexOf('/') != -1 || str.indexOf('"') != -1 
		|| str.indexOf('%') != -1 || str.indexOf('#') != -1 || str.indexOf('(') != -1 || str.indexOf(')') != -1){
