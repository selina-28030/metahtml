<script id="etu-recommender" type="text/javascript">
	function getLocation(){
		if (navigator.geolocation){
			navigator.geolocation.getCurrentPosition(getCurrentPosition, errorCallback ,positionOptions);
		}else{
			//alert("此瀏覽器不支援 Geolocation");
			return;
		}
	}

	var ua = navigator.userAgent || navigator.vendor || window.opera;
	if ( (ua.indexOf("FBAN") > -1) || (ua.indexOf("FBAV") > -1) ) {}
	else if ( ua.indexOf("Twitter") > -1 ) {}
	else if ( ua.indexOf("Line") > -1 ) {}
	else if ( ua.indexOf("Safari") > -1 && ua.indexOf("Chrome") == -1) {}
	else getLocation();

	var latitude = '';
	var longitude = '';
	var latCookieName = 'latitude';
	var lonCookieName = 'longitude';
	var latValue = getCookie(latCookieName);
	var lonValue = getCookie(lonCookieName);
	if(latValue != null && latValue.length > 0){
		latitude = latValue;
	}
	if(lonValue != null && lonValue > 0){
		longitude = lonValue;
	}
	function getCurrentPosition(position){
		console.log("pos:"+position);
		latitude = position.coords.latitude;
		longitude = position.coords.longitude;
		//domready(function() {
		//	$.cookie(latCookieName, latitude, { path: '/', expires: 1/24/2});
		//	$.cookie(lonCookieName, longitude, { path: '/', expires: 1/24/2});
		//});
	}
	//選用項目
	var positionOptions = {
		enableHighAccuracy: false,//高準確度
		timeout: 15000, //瀏覽器判斷位置時可用的時間長短。
		maximumAge: 0 //瀏覽器必需重新計算位置的最大時限
	};
	//錯誤處理器
	function errorCallback( error ) {
		// 不處理
		console.log("errPos:");
		console.log(error);
	}

	var sereff = '';
	if (document.referrer != "http://"+location.host+"/" || document.referrer != "http://"+location.host+"/index.html") {
		sereff = document.referrer;
	}
	else {
		sereff = getCookie('dreffer');
	}
	console.log('referrer: '+sereff);

	var atid = '';	
	var erUrlPrefix = 'https://rec.udn.com/';
	var logicid = '';

	//console.log('erUrlPrefix: '+erUrlPrefix);

	var _qevents = _qevents || [];
	var etuData = {
		group : 'udn',
		cid : 'udn_hard',
		act : 'view',
		cat : ['news','cms','','',''],
		uid : user_id,
		bid : '',
		pid : atid,
		page :'INDEX',
		title: '',
		etulogic: logicid,
		Rank_cat:'0',
		Rank_pid:'0',
		tag:[],
		keywords:[],
		referral : sereff,
		lat : getCookie(latCookieName),
		lon : getCookie(lonCookieName)
	};
	_qevents.push(etuData);

	(function() {
		var er = document.createElement('script');
		er.type = 'text/javascript';
		//er.async = true;
		er.defer = true;
		er.src = erUrlPrefix + 'er.js?' + (new Date().getTime());
		var currentJs = document.getElementById('etu-recommender');
		currentJs.parentNode.insertBefore(er, currentJs);
	})();
</script>
