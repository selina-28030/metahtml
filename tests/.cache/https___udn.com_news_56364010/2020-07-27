  <script>
function getCookie(name) {
	var value = "; " + document.cookie;
	var parts = value.split("; " + name + "=");
	if (parts.length == 2) return parts.pop().split(";").shift();
}

var _ga = getCookie('_ga');
var _gaA = _ga.split(".");
var google_id = '';
for (i=2;i<_gaA.length;i++) google_id = google_id+_gaA[i]+'.';
google_id = google_id.substring(0,google_id.length-1);
//console.log(google_id);
//if (typeof _ga !== 'undefined') var google_id = _ga.split(".")[2]+'.'+_ga.split(".")[3];
//else var google_id = '__';
var user_id = getCookie("udnland");
if (typeof user_id === 'undefined') user_id = 'noset';
var etu_id = getCookie("__eruid");

console.log('1.   etu_id: '+etu_id);
console.log('2.  user_id: '+user_id);
console.log('3.google_id: '+google_id);

</script>

