<script>
	var key = 1;
	var key2 = 1;
$(function(){
	$('.headerRight li a').hover(function(){
		key2 = 0;
		$('.headerRight li a').removeClass('curs');
		var asia = $(this).attr('tar');
		$(this).addClass('curs');
		$('#header2 p').css('display','none');
		$('.'+asia).css('display','block');
	},function(){
		key2 = 1;
		if(key == 1 && key2 == 1){
	$('#header2 p').css('display','none');
		}
	});
	$('#header').hover(function(){
		key = 0;
	},function(){
		key = 1;
		if(key == 1 && key2 == 1){
			$('.headerRight li a').removeClass('curs');
	$('#header2 p').css('display','none');
		}
	});

	$("#sideTabMenu li").click(function() {
		var num = $("#sideTabMenu li").index(this);
		$(".sideTabCont").addClass('disnon');
