<script type="text/javascript">
var vda_basepath = '/content/core/uppslagsverk/vissteduatt/';
var vda_file = vda_basepath+'vissteduatt.json';
var vda_imagepath = vda_basepath+'images/';
function defer(func) {
  if(typeof(window.jQuery) == "undefined")
    setTimeout(function(){
      defer(func);
    },50);
  else
    func();
}
function setVDA(elem,i,data) {
  /*
  elem.off("click.next");
  elem.on("click.next",function(e){
    setVDA(elem,(i+1)%data.length,data);
    return false;
    });
  */
  var row = data[i];
  elem.attr('href', row[1]);
  elem.find('.body').html("..."+row[0]);
  elem.find('img').attr('src', vda_imagepath+row[3]);
  elem.find('.byline').html(row[4]);
}
defer(function(){
  var vda_elems = $('[data-visste-du-att]');
  $.getJSON(vda_file, function(json){
    var index = Math.floor(new Date().getTime()/(24*3600*1000)-50)%json.length;
    vda_elems.each(function(i,e){
      setVDA($(e),index,json);
    });
  });
});
</script>

        
