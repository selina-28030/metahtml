<style>
#floating_banner_body {position:relative; margin:0 auto; width:960px;}
#floating_banner_left {position:absolute; width:300px; z-index:100; left:-260px; top:0px;}
#floating_banner_left2 {position:absolute; width:300px; z-index:100; left:-260px; top:610px;}
#floating_banner_right {position:absolute; width:160px; z-index:100; right:-240px; top:0px;}
#floating_banner_right2 {position:absolute; width:160px; z-index:100; right:-240px; top:610px;}
ul {list-style: none;}
</style>

<script>
jQuery(window).scroll(function()
    {
     jQuery("#floating_banner_left2").stop();
     jQuery("#floating_banner_right2").stop();
     if (jQuery(window).scrollTop() > 610) jQuery("#floating_banner_left2").animate({top:(jQuery(window).scrollTop() - 90) + "px"}, 100, "linear");
     else jQuery("#floating_banner_left2").animate({top:"610px"}, 100, "linear");
     if (jQuery(window).scrollTop() > 610) jQuery("#floating_banner_right2").animate({top:(jQuery(window).scrollTop() - 90) + "px"}, 100, "linear");
     else jQuery("#floating_banner_right2").animate({top:"610px"}, 100, "linear");
    });
</script>

