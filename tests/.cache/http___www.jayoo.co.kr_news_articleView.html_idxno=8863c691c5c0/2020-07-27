				data-desc="1000%EB%A7%8C%EB%AA%85%EC%9C%BC%EB%A1%9C%20%EC%B6%94%EC%A0%95%EB%90%98%EB%8A%94%20%EB%8C%80%EA%B7%9C%EB%AA%A8%20%EB%B0%98%EB%AC%B8%20%EC%8B%9C%EC%9C%84%20%EA%B5%B0%EC%A4%91%EC%9D%B4%20%EC%A7%91%EA%B2%B0%ED%95%9C%2010%EC%9B%94%209%EC%9D%BC%20%ED%95%9C%EA%B8%80%EB%82%A0%EC%9D%80%2036%EB%85%84%20%EC%A0%84%20%EC%95%84%EC%9B%85%EC%82%B0%20%ED%85%8C%EB%9F%AC%20%EC%82%AC%EA%B1%B4%EC%9D%B4%20%EC%9D%BC%EC%96%B4%EB%82%9C%20%EB%82%A0%EC%9D%B4%EC%97%88%EB%8B%A4.%20%EC%A7%80%EA%B8%88%EC%9D%80%20%EC%95%84%EB%AC%B4%EB%8F%84%20%EA%B8%B0%EC%96%B5%ED%95%98%EC%A7%80%20%EC%95%8A%EC%A7%80%EB%A7%8C%20%EC%95%84%EC%9B%85%EC%82%B0%20%EC%82%AC%EA%B1%B4%EC%9D%80%20%EB%B6%81%ED%95%9C%EC%9D%B4%201983%EB%85%84%2010%EC%9B%94%209%EC%9D%BC%20%EB%8B%B9%EC%8B%9C%20%EB%B2%84%EB%A7%88%28%ED%98%84%20%EB%AF%B8%EC%96%80%EB%A7%88%29%EB%A5%BC%20%EB%B0%A9%EB%AC%B8%20%EC%A4%91%EC%9D%B4%EB%8D%98%20%EC%A0%84%EB%91%90%ED%99%98%20%EB%8C%80%ED%86%B5%EB%A0%B9%20%EB%B0%8F%20%EC%88%98%ED%96%89%EC%9B%90%EB%93%A4%EC%9D%84%20%EB%8C%80%EC%83%81%EC%9C%BC%EB%A1%9C%20%EC%9E%90%ED%96%89%ED%95%9C%20%ED%85%8C%EB%9F%AC%EB%A5%BC%20%EB%A7%90%ED%95%9C%EB%8B%A4.%20%EB%8B%B9%EC%8B%9C%20%EC%A0%84%20%EB%8C%80%ED%86%B5%EB%A0%B9%EC%9D%98%20%EC%84%9C%EB%82%A8%EC%95%84%C2%B7%EB%8C%80%EC%96%91%EC%A3%BC%206%EA%B0%9C%EA%B5%AD%20%EA%B3%B5%EC%8B%9D%EC%88%9C%EB%B0%A9%20%EC%B2%AB%20%EB%B0%A9%EB%AC%B8%EA%B5%AD%EC%9D%B8%20%EB%B2%84%EB%A7%88%EC%9D%98%20%EC%95%84%EC%9B%85%EC%82%B0%20%EB%AC%98%EC%86%8C%EC%97%90%EC%84%9C%20%EC%9D%BC%EC%96%B4%EB%82%9C%20%EA%B0%95%EB%A0%A5%ED%95%9C%20%ED%8F%AD%EB%B0%9C%EB%A1%9C%20%EC%9D%B8%ED%95%B4%20%EB%8C%80%ED%86%B5%EB%A0%B9%EC%9D%98%20%EA%B3%B5%EC%8B%9D%C2%B7%EB%B9%84%EA%B3%B5%EC%8B%9D%20%EC%88%98%ED%96%89%EC%9B%90%2017%EB%AA%85%EC%9D%B4%20%EC%82%AC%EB%A7%9D%ED%95%98%EA%B3%A0%2C%2014%EB%AA%85%EC%9D%B4%20%EC%A4%91%EA%B2%BD%EC%83%81%EC%9D%84%20%EC%9E%85%EC%97%88%EB%8B%A4.%EC%9D%B4%20%EB%82%A0%20%EB%8C%80%ED%95%9C%EB%AF%BC%EA%B5%AD%EC%9D%98%20%EC%84%9C%EC%84%9D%EC%A4%80%20%EB%B6%80" 
				data-image="http://www.jayoo.co.kr/news/photo/201910/8863_15990_1041.jpg">
