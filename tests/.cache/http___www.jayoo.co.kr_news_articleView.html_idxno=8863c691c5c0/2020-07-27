			<button type="submit" class="support-btn">후원하기</button>
		</form>

		<script type="text/javascript">
			
			var support={
				SubmitSend:function(form)
				{

					// 후원 금액 체크 
					$(".chk_support_type").each(function(){
						var support_opt = "", price_free = 0;
						if ($(this).prop("checked") == true)
						{
							support_opt = $(this).val();
							switch (support_opt) 
							{
								case "A":
								case "B":
									form.price.value = $(this).attr("data-price");
									break;

								case "C":
									if (form.price_free.value <= 0 || form.price_free.value == 'NaN') form.price.value = $(this).attr("data-price");
									else 
									{
										price_free = $("#price_free").val().replace(/[^0-9]/g, '');
										price_free = (parseInt(price_free)>0) ? parseInt(price_free) : 0;
										form.price.value = price_free;
									}
									break;

								default:
									form.price.value = 0;
									break;
							} 
						}
					});

					if (parseInt(form.price.value) <= 0 || form.price.value == 'NaN')
					{
						alert("후원하실 방법을 선택해주세요");
						return false;
					}
					return true;
				}

				,evtCheckSupportType:function() 
				{
					$(".chk_support_type").on("click",function () {
						var $support_type = $(this).val();

						switch ($support_type)
						{
							case 'A':
								$("#price").val(1000);
								$(".label_free_payment").show();
								$(".input_free_payment").hide();
								break;

							case 'B':
								$("#price").val(5000);
								$(".label_free_payment").show();
								$(".input_free_payment").hide();
								break;
							
							case 'C':
								$("#price").val(50000);
								$("#price_free").val("50,000");
								$(".label_free_payment").hide();
								$(".input_free_payment").show();
								break;

							case 'D':
							case 'E':
								$("#price").val(0);
								$(".label_free_payment").show();
								$(".input_free_payment").hide();
								break;

							default:
								break;
						}
					});
				}

				,evtClickPrice:function()
				{
					$(".btn_click_price").on("click",function () {
						var click_price = price_free = total_price = 0;
						click_price = parseInt($(this).attr("data-price"));
						if (click_price == 0)
						{
							$("#price").val(50000);
							$("#price_free").val("50,000");
						}
						else
						{
							price_free = $("#price_free").val().replace(/[^0-9]/g, '');
							price_free = (parseInt(price_free)>0) ? parseInt(price_free) : 0;

							total_price = price_free + click_price;
							if(total_price > '490000'){
								alert("최대 490,000원까지 후원가능합니다.");
								return;
							}
							$("#price").val(total_price);

							var tmps = $("#price").val().replace(/[^0-9]/g, '');
							var tmps2 = tmps.replace(/(\d)(?=(\d\d\d)+(?!\d))/g, "$1,");
							$("#price_free").val(tmps2);
						}
					});
				}
			};

			$(function(){
				// 결제 옵션 선택 
				support.evtCheckSupportType();

				// 자유결제 금액 선택
				support.evtClickPrice();
			});
			
		</script>

	</div>
</div>
