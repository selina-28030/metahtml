				enabled: true,
				navigateByImgClick: true,
			},
			type: 'image',
		});

		$('.image-gallery a').on('click', function() {
			var magnificPopup = $.magnificPopup.instance;
			var href = $(this).attr('href')
			if (href === undefined) {
				magnificPopup.close();
				return false;
			}

			for (var key in magnificPopup.items) {
				if (magnificPopup.items[key].src == href) {
					magnificPopup.goTo(parseInt(key));
					return true;
				}
			};
		});
	
	
});
</script>
    </body>
</html>
