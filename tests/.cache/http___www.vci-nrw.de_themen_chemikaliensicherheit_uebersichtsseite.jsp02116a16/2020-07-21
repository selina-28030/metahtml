var isLoadingReachOptions = false;
function showLoading(){
if(!isLoadingReachOptions){
isLoadingReachOptions = true;
$("body").prepend("<div class='loading-indicator'></div>");
$("body").append("<div class='modal-wrapper' style='opacity: 1;'></div>");
}
}
function disableLoading(){
isLoadingReachOptions = false;
$(".modal-wrapper:first").remove();
$(".loading-indicator:first").remove();
}
$("#suche").on('input propertychange keypress', function(){
var value = $("#suche").val();
if(value && value !== ""){
$("#globalSearchSubmitBtn").prop( "disabled", false );
} else {
$("#globalSearchSubmitBtn").prop( "disabled", true );
}
});
$("#sf").on('input propertychange keypress', function(){
var value = $("#sf").val();
if(value && value !== ""){
$("#searchFormSubmitBtn").prop( "disabled", false );
} else {
$("#searchFormSubmitBtn").prop( "disabled", true );
}
});
var websiteCookies = document.cookie.split(';');
var cookieAlertAlreadyShown = false;
for(var i = 0; i < websiteCookies.length; i++) {
var c = websiteCookies[i];
while (c.charAt(0)==' ') {
c = c.substring(1);
}
if (c.indexOf("cookieAlertAlreadyShown=") == 0) {
cookieAlertAlreadyShown = true;
break;
}
}
if(!cookieAlertAlreadyShown){
$(".cookie-alert").removeClass("hidden");
}
$("button#reject-cookie").click( function( event ) {
document.cookie = "cookieAlertAlreadyShown=true; path=/";
document.cookie = "cookieDecision=no; path=/";
docCookies.setItem('et_oi_v2', 'no', null, '/', '.vci.de', false );
$(".cookie-alert").addClass("hidden");
return false;
});
$("button#accept-cookie").click( function( event ) {
document.cookie = "cookieAlertAlreadyShown=true; path=/";
document.cookie = "cookieDecision=yes; path=/";
docCookies.setItem('et_oi_v2', 'yes', null, '/', '.vci.de', false );
$(".cookie-alert").addClass("hidden");
return false;
});
var userCookieOptInAnswer = docCookies.getItem('cookieDecision');
var optInValue = docCookies.getItem('et_oi_v2');
if (userCookieOptInAnswer !== "yes") {
// No opt-in.
if (optInValue !== "no") {
console.log("optInValue !== no: " + optInValue);
docCookies.setItem('et_oi_v2', 'no', null, '/', '.vci.de', false );
}
}
