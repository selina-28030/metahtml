                        if (this.__type_name == "soft") {
                            o_form.f_name.value = value;
                        }
                        return true;
                    } else {
                        alert("è¯·è¾å¥å³é®å­");
                        return false;
                    }
                },
                on_foucs: function(obj) {
                    if (this.test_keywords(obj.value)) {
                        return;
                    } else {
                        obj.value = "";
                    }
