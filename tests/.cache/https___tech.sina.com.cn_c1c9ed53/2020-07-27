                window.techbrowser={
                    versions:function(){
                        var u = navigator.userAgent, app = navigator.appVersion;
                        return {
                            trident: u.indexOf('Trident') > -1, //IEåæ ¸
                            presto: u.indexOf('Presto') > -1, //operaåæ ¸
                            webKit: u.indexOf('AppleWebKit') > -1, //è¹æãè°·æ­åæ ¸
                            gecko: u.indexOf('Gecko') > -1 && u.indexOf('KHTML') == -1,//ç«çåæ ¸
                            mobile: !!u.match(/AppleWebKit.*Mobile.*/), //æ¯å¦ä¸ºç§»å¨ç»ç«¯
                            ios: !!u.match(/\(i[^;]+;( U;)? CPU.+Mac OS X/), //iosç»ç«¯
                            android: u.indexOf('Android') > -1 || u.indexOf('Adr') > -1, //androidç»ç«¯
                            iPhone: u.indexOf('iPhone') > -1 , //æ¯å¦ä¸ºiPhoneæèQQHDæµè§å¨
                            iPad: u.indexOf('iPad') > -1, //æ¯å¦iPad
                            webApp: u.indexOf('Safari') == -1, //æ¯å¦webåºè¯¥ç¨åºï¼æ²¡æå¤´é¨ä¸åºé¨
                            weixin: u.indexOf('MicroMessenger') > -1, //æ¯å¦å¾®ä¿¡ ï¼2015-01-22æ°å¢ï¼
                            qq: u.match(/\sQQ/i) == " qq" //æ¯å¦QQ
                        };
                    }(),
                    language:(navigator.browserLanguage || navigator.language).toLowerCase()
                }
        })();
