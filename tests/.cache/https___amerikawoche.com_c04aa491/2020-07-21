  function fadeIn(){
	showElement(crelementpos);
	crelement = document.getElementById('elemdivadd'+crelementpos);
    for (i = 0; i <= 1; i += (1 / steps)) {
      setTimeout("setOpacity(" + i + ")", i * duration);
    }
    crelementpos = crelementpos + 1;
    if(crelementpos > elemcount)
    	crelementpos = 1;
    timeoutID = setTimeout("fadeOut()", delay);
  }

  function fadeOut() {
    for (i = 0; i <= 1; i += (1 / steps)) {
      setTimeout("setOpacity(" + (1 - i) + ")", i * duration);
    }
    timeoutID2 = setTimeout("fadeIn()", duration);
  }

  function showElement(pos){
	for(i=1;i<=elemcount;i++){
		crelement = document.getElementById('elemdivadd'+i);
		countelement = document.getElementById('countdiv'+i);
		if (i==pos){
			setOpacity(0);
			crelement.style.display = 'block';
			countelement.style.backgroundColor = "#e5e5e5";
		}else{
			crelement.style.display = 'none';
			countelement.style.backgroundColor = "#FFFFFF";
		}
	}
  }
  function setDivPosition(pos){
	  clearTimeout(timeoutID);
	  clearTimeout(timeoutID2);
	  crelementpos = pos;
	  showElement(pos);
	  fadeIn();
  }
  fadeIn();
