  var elemcount = 3;
  var crelementpos = 1;
  var crelement;
  var duration = 500;  /* 1000 millisecond fade = 1 sec */
  var steps = 10;       /* number of opacity intervals   */
  var delay = 8000;     /* 8 sec delay before fading out */
  var timeoutID,timeoutID2;

  /* set the opacity of the element (between 0.0 and 1.0) */
  function setOpacity(level) {
    crelement.style.opacity = level;
    crelement.style.MozOpacity = level;
    crelement.style.KhtmlOpacity = level;
    crelement.style.filter = "alpha(opacity=" + (level * 100) + ");";
