<script>
    // timeout config for both PREBID and CRITEO CDB
    var HB_TIMEOUT = 1500;
    var PREBID_TIMEOUT = HB_TIMEOUT;
    var CRITEO_CDB_TIMEOUT = HB_TIMEOUT;
    var prebidTargerting = false;
    // callback function, No Need to Edit
    window.hb_status = {
        n_adserver_callback: [],
        adserverRequestSent: false
    };
    var n_adserver_callback = 0;
    var adServerCallback = function (source) {
        if (window.hb_status.adserverRequestSent) return;
        // add source to the processed list
        window.hb_status.n_adserver_callback.push(source);
        if (source == 'prebid') {
            googletag.cmd.push(function () {
                pbjs.que.push(function () {
                    prebidTargerting = true;
                    pbjs.setTargetingForGPTAsync();
                });
            });
        } else if (source == 'criteo') {
            googletag.cmd.push(function () {
                Criteo.SetDFPKeyValueTargeting();
            });
        }
        if (window.hb_status.n_adserver_callback.length >= 2 || "timeout" == source) {
            console.log('timeout check', window.hb_status.n_adserver_callback, source);
            googletag.cmd.push(function () {
                if (!prebidTargerting) {
                    pbjs.que.push(function() { 
                        pbjs.setTargetingForGPTAsync(); 
                    })
                }
                // pbjs.setTargetingForGPTAsync();
                googletag.pubads().refresh();
                window.hb_status.adserverRequestSent = true;
            });
        };
    }
</script>
<!-- Prebid and Criteo CDB Combined Section END-->

