<script>
 $(document).ready(function(){
	$.ajax({
		url:'https://money.udn.com/rssfeed/news/1001/12925',
		type:'GET',
		dataType:'xml',
		success:function(xml) {
			var myTitle;
			var myLink;
			var description;
			var titleArray = [];
			var linkArray = [];
			var desLinkArray = [];
			var $member_story_foot = $('#member_story_foot');
			var dt = $member_story_foot.find('dt').get();
			var imgLinkArray = [];
			var altTxtArray =[];
			var altTxt;
			$(xml).find('item').each(function(i){
				myTitle = $(this).children('title').text();
				titleArray.push(myTitle);
				myLink = $(this).children('link').text();
				linkArray.push(myLink);
				description = $(this).children('description').text();
				desLinkArray.push(description);
			});

			// 處理圖片連結，alt文字
			desLinkArray.forEach(function(item){
				var linkHead = item.indexOf('https');
				var linkFoot = item.indexOf('.jpg');
				var txtHead = item.lastIndexOf('<p>')
				var imageLink = item.substring(linkHead,linkFoot+4);
				imgLinkArray.push(imageLink);
				var pLast = item.lastIndexOf('</p>')
				if(pLast != -1) {
					altTxt = item.substring(txtHead+3,pLast);
				}else {
					altTxt = item;
				}
				altTxtArray.push(altTxt);
				
			}); 
			// 如果無圖，代入預設圖
			for(i=0; i<8; i++) {
				if(imgLinkArray[i].indexOf('.jpg') == -1){
					imgLinkArray[i] = 'https://pgw.udn.com.tw/gw/photo.php?u=https://udn.com/upf/2017_news/noimg_cid1001_1.jpg';
				}
			}
			// 將抓取到的資料一一填入
			for (i=0; i < 8; i++) {
				$(dt[i]).find('.member_story_title').text(titleArray[i]);
				$(dt[i]).find('a').attr('href',linkArray[i]);
				$(dt[i]).find('img').attr('src',imgLinkArray[i]);
				$(dt[i]).find('img').attr('alt',altTxtArray[i]);
			}
            /* 用文章連結判斷分類，代碼如下：標籤對照代碼（排除12926）
            新聞解析 12937 產業分析 12989 島嶼之外 12938 國內觀點 12952
            國際評論 13005 投資理財 12972 off學 12973 資訊圖表 12974  */
            for (var i = 0; i<8; i++) {
                if(linkArray[i].indexOf('12926') > 0 ) {
                    $(dt[i]).find('.news_tag').remove();
                }else if(linkArray[i].indexOf('12937') > 0) {
                    $(dt[i]).find('.news_tag').text('新聞解析');
                }else if(linkArray[i].indexOf('12952') > 0) {
                    $(dt[i]).find('.news_tag').text('國內觀點');
                }else if(linkArray[i].indexOf('12989') > 0) {
                    $(dt[i]).find('.news_tag').text('產業分析');
                }else if(linkArray[i].indexOf('12938') > 0) {
                    $(dt[i]).find('.news_tag').text('島嶼之外');    
                }else if(linkArray[i].indexOf('13005') > 0) {
                    $(dt[i]).find('.news_tag').text('國際評論');
                }else if (linkArray[i].indexOf('12972') > 0) {
                    $(dt[i]).find('.news_tag').text('投資理財');
                }else if (linkArray[i].indexOf('12973') > 0) {
                    $(dt[i]).find('.news_tag').text('off學');
                }else if (linkArray[i].indexOf('12974') > 0) {
                    $(dt[i]).find('.news_tag').text('資訊圖表');
                }
            }

		},
	});
 });
</script>
</div>
