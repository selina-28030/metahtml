<div id="google_ad">
<div id='div-gpt-ad-1570073101488-0'>
  <script>
    googletag.cmd.push(function() { googletag.display('div-gpt-ad-1570073101488-0'); });
  </script>
</div>
</div>

<div class="closeBtn"></div>

<script>
var isShow = true;
$(document).ready(function(){ 
        var adDiv = $('#bottomAd').children('#google_ad').children('div').css('display');
        var isAdDivHasContent = $('#bottomAd').children('#google_ad').children('div').find('iframe').length;
	$(window).scroll(function(){
		var Wheight = $(window).scrollTop();
		if (Wheight > 100 && isShow && adDiv !== 'none' && isAdDivHasContent > 0) {
			$('#bottomAd').addClass('show');
                        $('.closeBtn').addClass('show');
		}else {
			$('#bottomAd').removeClass('show');
                        $('.closeBtn').removeClass('show');
		}
	});
        $('.closeBtn').click(function(){
            $('#bottomAd').removeClass('show');
            $('.closeBtn').removeClass('show');
            isShow = false;
        });	
});

// 針對特殊廣告 讓自訂的closeButton 不出現
$(window).load(function(){
      if($('#MediaBook-AD2').length > 0 || $('.innity-apps-pull-up-touch-layer').length > 0) {
         $('.closeBtn').removeClass('show');
         isShow = false;
      }
       
});
</script>



</div>
<script>
	$(function() {
		$(window).on("scroll",function(e) {
			if ($(window).width() <= 749) {
				$("#bottomAd").css("display","block");
			}
		});
	});
</script>			<div id="show_box"></div>
