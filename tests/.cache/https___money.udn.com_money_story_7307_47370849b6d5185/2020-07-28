<script>
// 按鈕跟圖片隨機出現一組
$(document).ready(function(){

// 下載code
function randomString(len) {
　　var len = len || 8;
　　var $chars = "ABCDEFGHJKMNPQRSTWXYZabcdefhijkmnprstwxyz2345678";
　　var maxPos = $chars.length;
　　var pwd = "";
　　for (i = 0; i < len; i++ ) {
　　　　pwd += $chars.charAt(Math.floor(Math.random() * maxPos));
　　}
　　return pwd;
}
var isClick = true
      $('.member_click').on('click', function (e) {
        e.preventDefault()
        if (isClick) {
          isClick = false
          setTimeout(function () {
              isClick = true
          }, 5000)
        } else {
            return false
        }
        var pathArray = window.location.pathname.split('/')
        if (pathArray.length > 2) {
        if (pathArray[2] === 'index') {
            window.location =
            '/common2/downloadPdf?t=' +
            randomString() +
            '&pageType=index&artId=0&cateId=0&subcateId=0&extend=' + 'lpb_download'
        } else if (pathArray[2] === 'cate') {
            window.location =
            '/common2/downloadPdf?t=' +
            randomString() +
            '&pageType=cate&artId=0&cateId=' +
            $('meta[name=cate_id]').attr('content') +
            '&subcateId=0&extend=' + 'lpb_download'
        } else if (pathArray[2] === 'story') {
            window.location =
            '/common2/downloadPdf?t=' +
            randomString() +
            '&pageType=story&artId=' +
            pathArray[4] +
            '&cateId=' +
            $('meta[name=cate_id]').attr('content') +
            '&subcateId=' +
            pathArray[3] +
            '&extend=' + 'lpb_download'
        } else {
            window.location = '/common2/downloadPdf?t=' + randomString()
        }
        } else {
        window.location = '/common2/downloadPdf?t=' + randomString()
        }
        return false
        
      })
$(function() {
    if ($.urlParam('autoDownload') === 'y') {
        $(".member_click").trigger("click");
    }
})
})


