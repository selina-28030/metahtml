      /**
       * Gets current siteType depending on viewportWidth
       *
       * @returns {string} ("mobile", "tablet", "desktop")
       * @private
       */
      function getSiteType() {
        var viewportWidth = Math.max(document.documentElement.clientWidth, window.innerWidth || 0);
        if (viewportWidth >= 1001) {
          return "desktop";
        } else if (viewportWidth >= 641) {
          return "tablet";
        } else {
          return "mobile";
        }
      }
    
      /**
       * Is current page requested as autonative?
       *
       * @returns {boolean}
       * @private
       */
      function checkIsAutonative() {
        return !!window.location.search.match(/[&\?]+an=/gi);
      }
      
      window.GJ.advertising = {
        "isAutonative": checkIsAutonative(),
        "isIndexPage": true,
        "siteType": getSiteType(),
        "zone": checkIsAutonative() ? "_noad" : "foren"
      }
      
