backToTopButton.addEventListener("click", (event) => {
  window.scrollTo({
    top: 0,
    left: 0,
    behavior: 'smooth'
  });
});}</script>
</div>
  </div><!-- end col copyright -->
  </nav>
</div>
</footer>
