            try {
              posX = posX - target.getBoundingClientRect().left;
              posY = posY - target.getBoundingClientRect().top;
              target = target.contentDocument.elementFromPoint(posX, posY);
            } catch (err) {
              console.log(err);
              break;
            }
          }
          setTimeout(function () {
            this._simulateClick(target);
          }.bind(this), 1200);
        };
    
