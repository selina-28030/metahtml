<script type="text/javascript">
$(document).ready(function () {
	$(".weui_tabbar_item").eq(0).addClass("weui_bar_item_on");
	$('img').error(function(){
		$(this).attr('src', 'static/images/noimage.jpg');
    });
});
function close_search(){
	$("#search_bar").fadeOut();
}
function clear_search(){
	$("#search_input").val("");
}
function toggle_search(){
	if($("#search_bar").is(":visible")==true){
		$("#search_bar").fadeOut();
	}else{
		$("#search_bar").fadeIn();
	}
}
</script>
