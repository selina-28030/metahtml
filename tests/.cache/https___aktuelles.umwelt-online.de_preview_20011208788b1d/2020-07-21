    <script type="text/javascript">
      $(function () {

        $('.notify-popover').tooltip({
          placement: 'top',
          title: function () {
            return $(this).data('title');
          },
        });

        $('.notify-popover').popover({
          html: true,
          placement: function (popover, element) {

            var wrapper = $(element).parents('.dataTables_wrapper').first();
            var popoverTop = $(element).offset().top - 400;

            if (wrapper.offset().top > popoverTop) {
              return 'bottom';
            }

            return 'top';
          },
          container: 'body > div.wrapper > div.bottom div.widget.widget-table.lawchange-table > div.widget-content',
          title: function () {
            return $(this).data('title') + '<span class="close" style="margin-top: -5px">&times;</span>';
          },
        }).on('shown.bs.popover', function (e) {
          var popover = $(this);
          var current_popover = popover.data('bs.popover').tip();
          current_popover.find('.close').on('click', function (e) {
            popover.click();
          });
        });
        $('#notify_types').bootstrapSwitch();
      });
    </script>
