    var mob_links = [{"name":"Ancient History","url":"http:\/\/traffic.pubexchange.com\/l\/15b878e1-1b2f-4cf3-8135-6d1c87f44f7a\/619428e5-3c3f-46e9-92d1-e495163a146c\/3c22f284-34d0-4f1d-b1c3-89dee37eb148\/f?u=ancient.eu","headline":"Achilles","thumb":"https:\/\/www.ancient.eu\/img\/r\/p\/500x600\/3351.jpg?v=1485680992","type":"pubex"},{"name":"Ancient Origins","url":"http:\/\/traffic.pubexchange.com\/l\/15b878e1-1b2f-4cf3-8135-6d1c87f44f7a\/74176c14-3046-43b5-b550-14830d933a53\/43a6e4e8-b219-483a-8fb6-1aa3cc5ea973\/f?u=ancient-origins.net","headline":"How 'Death Ships' Spread Disease Through the Ages","thumb":"https:\/\/www.ancient-origins.net\/sites\/default\/files\/styles\/slideshow\/public\/field\/image\/death-ships.jpg","type":"pubex"},{"name":"Ancient Origins","url":"http:\/\/traffic.pubexchange.com\/l\/15b878e1-1b2f-4cf3-8135-6d1c87f44f7a\/74176c14-3046-43b5-b550-14830d933a53\/087d5c00-ee90-4731-a341-f17c09d35c37\/f?u=ancient-origins.net","headline":"45,000-Year-Old Weapons Discovered In Sri Lankan Cave","thumb":"https:\/\/www.ancient-origins.net\/sites\/default\/files\/styles\/slideshow\/public\/field\/image\/Oldest-bow%E2%80%93and-arrow.jpg","type":"pubex"}];
    pbh_config.pubex_list = pbh_config.is_mobile ? mob_links : dsk_links;

    var pbh_links = [{"name":"trending","url":"https:\/\/allthatsinteresting.com\/king-leopold-ii-congo","headline":"Why Isn\u2019t Belgium\u2019s King Leopold II As Reviled As Hitler Or Stalin?","thumb":"https:\/\/allthatsinteresting.com\/wordpress\/wp-content\/uploads\/2016\/09\/leopold-children.jpg","type":"internal"},{"name":"trending","url":"https:\/\/allthatsinteresting.com\/dolmen-de-guadalperal","headline":"Drought Uncovers \u2018Spanish Stonehenge\u2019 Hidden Underwater For Decades","thumb":"https:\/\/allthatsinteresting.com\/wordpress\/wp-content\/uploads\/2019\/09\/spanish-dolmen.jpg","type":"internal"}];
    //window.pbh_outbrain_def = pbh_get_outbrain();
    var thumbnail_server = "allthatsinteresting.com";

    var get_rand_int = function(x) { return Math.floor(Math.random() * x); };
    var get_rand_item = function(ls) {
      if (!ls || !ls.length)
        return false;

      return ls[get_rand_int(ls.length)];
    };

    var is_user_anglo = function() {
      return 'US,UK,CA,AU'.indexOf(pbh_config.country_code) != -1;
    };

    var get_link_html = function(link) {
        var is_internal = link.name == 'trending' || link.name == "All That's Interesting";
        var promo = is_internal ? 'Trending on ATI' : 'Promoted by ' + link.name;
        var no_follow = is_internal ? '' : 'rel="nofollow"';
        if (!link.thumb) {
          console.error('bad link', link);
        }
        if (link.thumb && link.thumb.indexOf('//') === 0)
          link.thumb = 'http:' + link.thumb;

        return '<li class="content-item">' +
          '<a ' + no_follow + ' href="' + link.url + '" class="content-link ' + (link.type || '') + '">' +
          '<div class="img-wrap"><img onerror="pbh_imgerr(this)" data-src="//' + thumbnail_server + '/thumb/290.240.' +
              encodeURIComponent(link.thumb) + '" alt="" class="img-wrap dataload" /></div>' +
          '<div class="details"><div class="title">' + link.headline + '</div>' +
          '<div class="promoted-by">' + promo + '</div>' +
          '</div></a></li>';
    };

    var shuffle = function(arr) {
      var currentIndex = arr.length, temporaryValue, randomIndex;

      // While there remain elements to shuffle...
      while (0 !== currentIndex) {

        // Pick a remaining element...
        randomIndex = get_rand_int(currentIndex);
        currentIndex -= 1;

        // And swap it with the current element.
        temporaryValue = arr[currentIndex];
        arr[currentIndex] = arr[randomIndex];
        arr[randomIndex] = temporaryValue;
      }

      return arr;
    };

    $(function() {
        var link_list;

        if (is_user_anglo()) {
          link_list = pbh_links.slice(0, 2);
          link_list.push(get_rand_item(pbh_config.pubex_list));
        } else {
          link_list = pbh_config.pubex_list.slice(0, 2);
          link_list.push(get_rand_item(pbh_links));
        }

        shuffle(link_list);

        var html = link_list.reduce(function(html, link) {
          return html + get_link_html(link);
        }, '');
        $('.around-the-web-section .content-items').empty().append(html);
        $('.dataload').Lazy();
    });
  })();
  </script>
