    <script>
        function isInViewport(el) {
            if (!el) {
                return false;
            }

            var bounding = el.getBoundingClientRect();
            var elIsInViewport =
                bounding.top >= 0 &&
                bounding.left >= 0 &&
                bounding.bottom <=
                    (window.innerHeight || document.documentElement.clientHeight) &&
                bounding.right <=
                    (window.innerWidth || document.documentElement.clientWidth);
            
            return elIsInViewport;
        }

        function loadTalkScript(siblingNode) {
            var coralLoader = document.createElement("script");
            coralLoader.async = true;
            coralLoader.src = "https://talk.allthatsinteresting.com/static/embed.js";
            coralLoader.onload = function() {
                Coral.Talk.render(document.getElementById("coral_talk_stream"), {
                    talk: "https://talk.allthatsinteresting.com/",
                    asset_id: "87540",
                    asset_url: "https://allthatsinteresting.com/north-korea-assassination-plans"
                });
            };

            siblingNode.parentNode.insertBefore(coralLoader, siblingNode.nextSibling);
        }

        
        var scriptLoaded = false;
        var targetEl = document.getElementById("coral_talk_stream");

        function loadScriptIfCommentSectionVisible() {
          if(isInViewport(targetEl) && !scriptLoaded) {
            loadTalkScript(targetEl);
            scriptLoaded = true;
          }
        }

        function loadScriptWhenBrowserIdle(scriptLoaderFn) {
          return function() {
            if ('requestIdleCallback' in window) {
              requestIdleCallback(scriptLoaderFn)
            } else {
              scriptLoaderFn()
            }
          }
        }

        window.addEventListener("scroll", loadScriptWhenBrowserIdle(loadScriptIfCommentSectionVisible));
    </script>
