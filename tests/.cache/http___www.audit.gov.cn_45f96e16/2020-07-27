<script>

 function rigScroll(){
        var winWidth=$(window).width();
        if(winWidth<1024){
            $(".is_mulu").css({"right":10})
        }else{      
            var winRig=(winWidth-1005)/2-90;
            $(".is_mulu").css({"right":winRig})
        }
  $(".closeBtn").click(function(){
   $(".is_mulu").hide()
  })
  
    }
 rigScroll()
 $(window).scroll(function() {
  rigScroll()
 });
 
 $(window).resize(function() {
  rigScroll()
 }); 

</script>
