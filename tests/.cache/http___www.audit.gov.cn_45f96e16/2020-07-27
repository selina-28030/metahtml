function setTabc(name,cursel,n){
for(i=1;i<=n;i++){
var menu=document.getElementById(name+i);
var con=document.getElementById("con_"+name+"_"+i);
var fr=document.getElementById("fr_"+name+"_"+i);
menu.className=i==cursel?"red16_30b":"grey16_30b";
con.style.display=i==cursel?"block":"none";
fr.src=cursel==1?"mtsd_index.html":"qwjd_index.html";
}
}
</script>

