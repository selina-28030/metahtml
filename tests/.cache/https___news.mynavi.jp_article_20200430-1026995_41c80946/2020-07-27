<script type="text/javascript">
function rotateAsyncAd(ids) {
  var contentsTextAd = new Array(ids.length);
  for (var i = 0; i < ids.length; i++) {
    contentsTextAd[i] = document.getElementById(ids[i]).innerHTML;
  }
  contentsTextAd.sort( function() { return Math.random() - 0.5; } );

  for (var i = 0; i < ids.length; i++) {
    document.getElementById(ids[i]).innerHTML = contentsTextAd[i];
  }
}
rotateAsyncAd(['wrap_rankAd_a', 'wrap_rankAd_b']);
</script>

