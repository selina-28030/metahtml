    BiddingManager.que.push(function(bm) {
      // JSONの内容を適用するために必要
      bm.setConfig();

      // JSONで定義された広告情報を有効化前に改変する
      bm.mapSlot(function(slot) {
        // すべてのslotのKeyValueに動的な値を追加
        slot.keyValues = slot.keyValues || {};
        if (slot.keyValues) {
          slot.keyValues.ad_group = Adomik.randomAdGroup();
          slot.keyValues.ad_h = (new Date).getUTCHours().toString();
        }

        // 必ず改変後のslotを返す
        return slot;
      });

      //1次リリース用（2次リリース時に削除する）
      // no-ad
      var is_no_ad = false;
      if(!news_vars.ad && !news_vars.brandgate && !news_vars.categoryjack && news_vars.ptype && news_vars.ch != "hisamitsu")is_no_ad = true;
      // article
      var is_article = false;
      if(news_vars.ptype !== "undefined" && !news_vars.ptype.match(/^top.*/))is_article = true;
      // image
      var is_large_image = false;
      if(news_vars.displayForm !== "undefined" &&  news_vars.displayForm == "image")is_large_image = true;
      // top
      var is_top = false;
      if(news_vars.ptype == "top")is_top = true;
      // ch_top
      var is_topch = false;
      if(news_vars.ptype == "top-ch")is_topch = true;
      // cat_top
      var is_topcat = false;
      if(news_vars.ptype == "top-cat")is_topcat = true;

      var ids = []

      if (!is_top && !is_large_image && is_no_ad && document.referrer != location.href && !document.referrer.match(/(news|headlines)\.yahoo\.co\.jp/)) {
        // mn.welcome
        ids.push('div-gpt-ad-1479185899023-20');
      }
      if (!is_topch && !is_top && (is_no_ad || news_vars.categoryjack && !news_vars.ad || news_vars.cat == "tokyo2020" || news_vars.cat == "cashless")) {
        // mn.billboard
        ids.push('div-gpt-ad-1466143984602-0');
      }
      if (!is_topcat && !is_topch && !is_top && !is_large_image && (is_no_ad || news_vars.categoryjack && !news_vars.ad && is_article)) {
        // mn.inread
        ids.push('div-gpt-ad-1466143984602-1');
      }
      if (!is_top) {
        // mn.reca
        ids.push('div-gpt-ad-1479185899023-14');
      }else{
        // mn.reca-top
        ids.push('div-gpt-ad-1593574342922-0');
      }
      if (!is_top) {
        // mn.recb
        ids.push('div-gpt-ad-1479185899023-16');
      }
      if (is_article && !is_large_image) {
        // mn.recl
        ids.push('div-gpt-ad-1479185899023-18');
        // mn.recr
        ids.push('div-gpt-ad-1479185899023-19');
      }
      if (!is_topch && !is_top && is_no_ad) {
        // mn.overlay
        ids.push('div-gpt-ad-1507249628623-0');
      }
      if (!is_top && !is_large_image && is_article && (is_no_ad || news_vars.brandgate || news_vars.categoryjack && !news_vars.ad)) {
