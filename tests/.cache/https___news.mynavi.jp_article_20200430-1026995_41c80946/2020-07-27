        ids.push('div-gpt-ad-1479185899023-0');
      }
      if (!is_topch && !is_top && !is_large_image) {
        // mn.recc
        ids.push('div-gpt-ad-1485738116644-0');
      }
      if (!is_top && !is_large_image && is_no_ad) {
        // mn.gateside
        ids.push('div-gpt-ad-1479185899023-6');
      }
      if (!is_topch && (is_no_ad || news_vars.brandgate || news_vars.categoryjack && !news_vars.ad)) {
        // mn.footer
        ids.push('div-gpt-ad-1479185899023-4');
      }
      if (!is_topch && !is_top && !is_large_image && (is_no_ad || news_vars.brandgate || news_vars.categoryjack && !news_vars.ad)) {
