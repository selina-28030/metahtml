var myAnchor;
 jQuery(document).ready(function() {

 var myurl = document.URL;
 var mysearch = myurl.search(/#/);
 var urlLength = myurl.length;

var myFile = document.location.toString();
if (myFile.match('#')) { // the URL contains an anchor
  // click the navigation item corresponding to the anchor
  myAnchor = myFile.split('#')[1];
}
if(mysearch != -1) {
    jQuery('#'+myAnchor+' .ka-content').css({"display":"block"});
    jQuery('#'+myAnchor).parent().removeClass("close").addClass("open");
    setTimeout("window.scrollTo(0,jQuery('#'+myAnchor).position().top)", 10);
}
jQuery('.internal-link').click(function() {
     var myFile = document.location.toString();
     var myFile2 = myFile+jQuery(this).attr('href');
    
    if (myFile2.match('#')) { // the URL contains an anchor

     window.location.href = myFile2;
     location.reload(true);
 
    }
   });
   
});
