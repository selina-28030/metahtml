        var stableUserId = window.Zeit.cookieRead( 'creid' ) || window.navigator.userAgent;
        var hashString = salt + stableUserId;
        var index = window.Zeit.simpleHash( hashString ) % ( choices.length );
        return choices[ index ];
    };
    
