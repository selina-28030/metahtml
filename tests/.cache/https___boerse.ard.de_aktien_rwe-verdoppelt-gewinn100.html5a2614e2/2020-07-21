<script type="text/javascript">
loaded(['jquery', 'ardboerse'], function(){
jQuery(document).ready(function() {
var count = 0;
if(jQuery.cookie("datapolicy-Youtube")){
loadAll();
} else {
jQuery('.dsgvoYoutube').css("display", "block");
var i;
for(i=0; i < count; i++) {
jQuery("#dsgvo-btn-" + i).click({id:i}, loadService);
}
}
function loadService(event){
//checkbox checken:
if(jQuery('#dataPolicyAcceptPermanently-'+event.data.id).prop('checked')){
//cookie setzen und ALLE Youtube-Elemente erzeugen
jQuery.cookie("datapolicy-Youtube", "true", { path: '/', expires: 3650 });
jQuery('.dsgvoYoutube').hide();
loadAll();
}else{
//DIESES Youtube-Element erzeugen:
jQuery('#dsgvo-overlay-'+event.data.id).hide();
var elementId = "externalService-" + event.data.id;
var container = document.getElementById(elementId);
var url = container.dataset.serviceUrl;
jQuery("#"+elementId).html("<iframe id='dsgvo-iframe-"+ event.data.id+"' class='youtubeVideoContent' src='"+url+"' frameborder='0' allow='autoplay;encrypted-media' allowfullscreen></iframe>");
jQuery('#dsgvo-iframe-'+event.data.id).show('slow');
jQuery('.sectionArticle').css("height", "100%");
}
}
function loadAll() {
var i;
for(i=0; i < count; i++) {
var elementId = "externalService-" + i;
var container = document.getElementById(elementId);
var url = container.dataset.serviceUrl;
jQuery("#"+elementId).html("<iframe id='dsgvo-iframe-"+i+"' class='youtubeVideoContent' src='"+url+"' frameborder='0' allow='autoplay;encrypted-media' allowfullscreen></iframe>");
jQuery('#dsgvo-iframe-'+i).show('slow');
jQuery('.sectionArticle').css("height", "100%");
}
}
//opt-out für Zählpixel---------------------------------
var measuringSystems = ["agf","ati","ivw"];
for(var i=0; i < measuringSystems.length; i++) {
if(jQuery.cookie('tracking') == null){
jQuery('#dataPolicy'+measuringSystems[i].toUpperCase()).prop('checked', false);
}else{
jQuery('#dataPolicy'+measuringSystems[i].toUpperCase()).prop('checked', !hasTrackingCookie(measuringSystems[i]));
}
}
for(var i=0; i < measuringSystems.length; i++) {
jQuery('#dataPolicy'+measuringSystems[i].toUpperCase()).on("click", {measuringSystem:measuringSystems[i]}, handleMeasuringCookie);
}
function handleMeasuringCookie(event){
var cookieContent = {agf:!jQuery('#dataPolicyAGF').prop('checked'),ati:!jQuery('#dataPolicyATI').prop('checked'),ivw:!jQuery('#dataPolicyIVW').prop('checked')};
document.cookie = 'tracking='+ JSON.stringify(cookieContent) +';max-age=315360000;domain=.boerse.ard.de;path=/';
}
function jqDocwrite(html) {
root.html(root.html() + html);
}
});
});
</script>
