    <script>

 var push_url = 'https://www.zaobao.com.sg/sph-reward-points/get-data';
  $.post(push_url,
  function(push_data, push_status){
if(push_data != '') {
    var rewards_environment = 'https://rewards.zaobao.com.sg/';
      var total_balance = JSON.parse(push_data);
      if(push_status == 'success' && total_balance != 'undefined') {
         // Append Rewards Point Div inside share widgets.
        if($(document).width() > 500) {
          jQuery('<div class="aside-social-icon d-lg-block"><a class="sidebar-tracking" id="rwd-sidebar" target="_blank" href="'+rewards_environment+'?utm_source=sidebar" noopener><span class="a2a_svg a2a_s__default" style="background-color: transparent;"><picture><source srcset="/sites/all/modules/custom/sph_reward_points/images/rewards_point_white.png" media="(max-width: 500px)"><source srcset="/sites/all/modules/custom/sph_reward_points/images/rewards_point_updated.png" media="(min-width: 800px)"><img class="rewards-point-image" src="/sites/all/modules/custom/sph_reward_points/images/rewards_point_updated.png" onmouseover="this.src=\'/sites/all/modules/custom/sph_reward_points/images/rewards_point.gif\'" onmouseout="this.src=\'/sites/all/modules/custom/sph_reward_points/images/rewards_point_updated.png\'" alt="" style="width:100%;"></picture></span><div class="rewards-point-logo" style="color: #A50034;font-size: 14px;font-weight: bold;text-align: center;">'+formatNumber_block(total_balance)+'</div></a></div>').appendTo('.a2a_kit');
         }
        jQuery('.rewards-point-logo').text(formatNumber_block(total_balance));
      }
}
  });
function formatNumber_block(num) {
    return num.toString().replace(/(\d)(?=(\d{3})+(?!\d))/g, '$1,')
