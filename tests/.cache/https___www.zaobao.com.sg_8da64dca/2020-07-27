function trySingleSignOnLogin() {
      var f = document.createElement('iframe');
      f.src = "/p/login_sso.php"; 
      f.style.width = "0px"; 
      f.style.height = "0px";
      f.style.border = "none";
      jQuery('body').append(f);
    }

    function loadSubscriberContent() {
      jQuery.post( "/p/login_details?article_id="+_data.articleid, function( data ) {
        
        data = JSON.parse(data);
        // Login & Logout :)
         _data.visitorcat = data.service_type_value;
         _data.ac =  data.service_type_value;
        if ( (data.usertype == 'sub' || data.usertype == 'reg') && data.login_id != "") {
        	var zb_login_info = '<span class="welcome">欢迎,&nbsp;</span><li class="nav-user user user-dropdown dropdown row no-gutters"><a href="javascript:;" class="dropdown-toggle" name="login-user-name" data-toggle="dropdown" aria-expanded="false"><span id="sph_user_name">'+data.login_id+'</span></a><div class="col-12 dropdown-menu"><div class="topnav-submenu"><div class="topnav-link dropdown-item"><a class="mysph_user_name" href="javascript:;">更新密码</a></div><div class="topnav-link dropdown-item"><a class="mysph_logout" href="javascript:;">退出</a></div></div></div></li>';
		 
          jQuery('#login-info').html(zb_login_info);
          jQuery('#login-info-mobile').html(zb_login_info);
          if(data.ldapw){
            _data.at = data.ldapw;
            var meta = document.createElement('meta');
            meta.name = 'ldapw';
            meta.content = data.ldapw;
            document.getElementsByTagName('head')[0].appendChild(meta);
          }
          var regex = RegExp('/story[0-9]{8}-[0-9]+');
          var current_page_url = window.location.pathname;
          if(regex.test(current_page_url)) {
            location.reload();
          }
        } else if(data.usertype == "anoy") {
          var zb_login_info = '<a class="cta cta-minw cta-bordered cta-grey mysph_login"  href="javascript:;">登录</a><a class="cta cta-minw cta-solid cta-gradient-red" href="https://www.sphsubscription.com.sg/eshop/?r=products/newsubscriptionpackages&pcode=zb&utm_campaign=zb_subcription&utm_medium=sph-publication&utm_source=zb&lang=zh&utm_content=homepageheader" target="_blank">订阅</a>';
