	parentTargetDiv.insertBefore(gmoDiv, targetDiv.nextSibling);
	parentTargetDiv.insertBefore(gmoScript, targetDiv.nextSibling);
	console.log("%c Recommned-center ",recommendAdLabel," ads : TAXEL insert");
}
	
