<script>
function initialNativenewsAds(iseRetry) {
	if(typeof iseRetry === 'undefined') iseRetry = 0;
	// console.log("%c ISE ",iseAdLabel," info : initialIseAds " + iseRetry);
	if (iseRetry > 10) {
		console.log("%c Dailysports ",iseAdLabel," error : initialNativenewsAds retry over");
		return;
	}
	if (document.getElementById('mainGenreList').childElementCount) {
		mainGenreListElement = document.getElementById("mainGenreList");
    
    var popinRecommendDiv = document.createElement('div');
    popinRecommendDiv.id = '_popIn_recommend_news';
    mainGenreListElement.insertBefore(popinRecommendDiv, mainGenreListElement.children[4]);
	} else {
		// console.log("%c ISE ",iseAdLabel," info : csw-content-box missing");
		setTimeout(function() {
			initialNativenewsAds(++iseRetry);
		}, 100);
	}
}
initialNativenewsAds();
</script>

