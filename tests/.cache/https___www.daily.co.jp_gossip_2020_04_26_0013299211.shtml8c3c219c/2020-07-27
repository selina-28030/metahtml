<script>
var recommendAdLabel = 'color: #000; background: #fff100; padding: 1px 4px; border-radius: 3px;';
var targetDiv = document.getElementById("dspc-inread");
var parentTargetDiv = targetDiv.parentNode;
	
function initialRecommendCenterAds(){
	adsSelectFlg = getRandomIntInclusive(1,2);
	if(adsSelectFlg === 1){
		setRecommendCenterTaxelAds();
	}else{
		setRecommendCenterPopinAds();
	}
}

function getRandomIntInclusive(min, max) {
  min = Math.ceil(min);
  max = Math.floor(max);
  return Math.floor(Math.random() * (max - min + 1)) + min;
}

function setRecommendCenterTaxelAds(){
	var gmoDiv = document.createElement('div');
	gmoDiv.id = 'gmo_rw_13384';
	gmoDiv.setAttribute("data-gmoad" , "rw");

	var gmoScript = document.createElement('script');
	gmoScript.type = "text/javascript";
	gmoScript.async = true;
