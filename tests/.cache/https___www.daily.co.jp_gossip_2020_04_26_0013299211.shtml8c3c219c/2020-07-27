function uzouRecommend(){
	var recommendSection = document.createElement('section');
	recommendSection.classList.add("section");

	var recommendDiv = document.createElement("div");
	recommendDiv.classList.add("ttl-section");

	var recommendh1 = document.createElement("h1");
	recommendh1.innerHTML = 'ãã®è¨äºããªã¹ã¹ã¡';

	var uzouRecommendScript = document.createElement('script');
	uzouRecommendScript.src = "//speee-ad.akamaized.net/tag/3-daily_pc/js/outer-frame.min.js";
	uzouRecommendScript.type = "text/javascript";
	uzouRecommendScript.charset = "utf-8";

	var uzouRecommendDiv = document.createElement("div");
	uzouRecommendDiv.classList.add("uz-3-daily_pc");
	uzouRecommendDiv.classList.add("uz-ny");

	recommendSection.appendChild(recommendDiv);
	recommendDiv.appendChild(recommendh1);
	recommendSection.appendChild(uzouRecommendScript);
	recommendSection.appendChild(uzouRecommendDiv);

	var targetDiv = window.top.document.getElementsByClassName('primaryContents');
	targetDiv[0].appendChild(recommendSection);
}

function zucksRecommend(){
	var zucksRecommendScript = document.createElement('script');
	zucksRecommendScript.src = "https://j.zoe.zucks.net/zoe.min.js";
	zucksRecommendScript.defer = true;

	var zucksRecommendBody = document.createElement("script");
	zucksRecommendBody.type = 'text/javascript';
	zucksRecommendBody.innerText = '(window.gZgokZoeQueue = (window.gZgokZoeQueue || [])).push({ frameId: "_d4b6d3df97" });(window.gZgokZoeWidgetQueue = (window.gZgokZoeWidgetQueue || [])).push({frameId: "_d4b6d3df97", parent: "#zucks-widget-parent_d4b6d3df97" });';

	var zucksRecommendDiv = document.createElement("ins");
	zucksRecommendDiv.id  = "zucks-widget-parent_d4b6d3df97";

	var targetDiv = window.top.document.getElementsByClassName('primaryContents');
	targetDiv[0].appendChild(zucksRecommendScript);
	targetDiv[0].appendChild(zucksRecommendBody);
	targetDiv[0].appendChild(zucksRecommendDiv);
}
</script>




