<script type="text/javascript">
  var url = document.location.href;
  url = url.replace(/https?:\/\//gi, '');

  var host = url.substring(0, url.indexOf('.'));
  if (url.indexOf('blog.excite.co.jp') >= 0) {
    host = 'blog';
  }

  var path = '';
  if (url.indexOf('/') >= 0) {
    path = url.substring(url.indexOf('/') + 1, url.length);
    if (path.indexOf('.asp') >= 0) {
      path = path.substring(0, path.indexOf('.asp') + 4);
    } else if (path.lastIndexOf('/') >= 0) {
      path = path.substring(0, path.lastIndexOf('/'));
    }
  }

  var query = '';

  // set SP
  var sp;
  if (host == 'blog') {
    sp = '/exblog/' + host + '/' + path;
  } else {
    sp = '/exblog/users/' + host + '/' + path;
  }
  sp = escape(sp);
  sp = sp.replace(/\//gi,'%2F');
  query += 'SP=' + sp;

  // set SD
  var referrer = document.referrer;
  if (/^https?:\/\//.test(referrer)) {
    query += '&SD=' + referrer;
    // set SV
    if (/[?&](kw|MT|name|p|q|qt|query|search|word)=([^&]+)/.test(referrer)) {
      query += '&SV=' + encodeURIComponent(RegExp.$2);
    }
  }

    // set SC
  var tags = [];
    if (tags.length > 0) {
    query += '&SC=' + encodeURIComponent(encodeURIComponent(tags.join("\t")));
  }
  
  query += '&rnd=' + Math.random();

  new Image().src = 'https://logping1.exblog.jp/ping?' + query;
</script>
<!--suppress ALL -->
<script type="text/javascript">
  jQuery(function ($) {
    var url = document.location.href;
    var matches = url.match(/^https?:\/\/[^\/]+(\/.*)$/i);
    if (!matches) {
      return;
    }

    var query = '';
    var path = matches[1];
        if (url.indexOf('blog.excite.co.jp') >= 0) {
      matches = path.match(/^\/[^\/]+(.*)$/);
      if (matches) {
        path = matches[1];
      }
    }
    var agent = navigator.userAgent;

    var addQuery = function (key, value) {
      if (query) {
        query += '&';
      }
      query += key + '=' + encodeURIComponent(value);
    };

    var getVisitedIds = function () {
      var visited = $.cookie('v');
      if (!visited) {
        return [];
      }

      visited = visited.replace('vid=', '');
      return visited.split('\f');
    };

    var hasVisited = function () {
      var visitedIds = getVisitedIds();
      if (!visitedIds) {
        return false;
      }

      return visitedIds.indexOf('d0364262') >= 0;
    };

    var setExblogCookie = function () {
      var domain = '';
      if (url.indexOf('blog.excite.co.jp') >= 0) {
        domain = '.excite.co.jp';
      } else {
        domain = '.exblog.jp';
      }

      var date = new Date();
      var expires = new Date(date.getFullYear(), date.getMonth(), date.getDate(), '23', '59', '59');

      var visitedIds = getVisitedIds();
      visitedIds.push('d0364262');
      var vid = 'vid=' + visitedIds.join('\f');
      $.cookie('v', vid, {
        expires: expires,
        path: '/',
        domain: domain,
        secure: false
      });
    };

    addQuery('path', path);
    addQuery('UA', agent);
    addQuery('blog', 'd0364262');
    addQuery('user', 'guest');
            if (hasVisited()) {
      addQuery('visited', 1);
    } else {
      setExblogCookie();
    }

    new Image().src = 'https://api.exblog.jp/count/uu/?' + query;

  });

</script>
