<script>
TKOLIB.adBidders = {APS: true, PBJS: true, IAS: true};
TKOLIB.adAtfLoad = function(bidder) {
  if (false === bidder in TKOLIB.adBidders) {
    return false;
  } else if (false === TKOLIB.adAtfLoaded() && false === TKOLIB.adIasLoaded) {
    TKOLIB.adIasLoad();
  }
  delete TKOLIB.adBidders[bidder];
  if (0 !== Object.keys(TKOLIB.adBidders).length || true === TKOLIB.adAtfLoaded()) {
    return false;
  }
  googletag.cmd.push(function() {
    googletag.pubads().refresh(TKOLIB.adSlots.atf,{changeCorrelator: false});
    googletag.pubads().refresh(TKOLIB.adSlots.btf,{changeCorrelator: false});
  });
  TKOLIB.AdsLazyload();
};
TKOLIB.adAtfLoaded = function() {
  if (window.googletag && googletag.apiReady) {
    return (null === TKOLIB.adSlots.atf[0].getResponseInformation()) ? false : true;
  } else {
    return null;
  }
};
TKOLIB.adIasLoad = function() {
  var iasPETSlots = [];
  googletag.pubads().getSlots().forEach(function(slot) {
    if (-1 !== ['div-gpt-ad-smp-f-center-1', 'div-gpt-ad-smp-f-center-1S', 'div-gpt-ad-smp-d-center-0', 'div-gpt-ad-rec1', 'div-gpt-ad-rec2'].indexOf(slot.getSlotElementId())) {
      var sizes = slot.getSizes().map(function(size) {
        return (size.getWidth && size.getHeight) ? [size.getWidth(), size.getHeight()] : [1, 1];
      });
      iasPETSlots.push({
        adSlotId: slot.getSlotElementId(),
        size: sizes,
        adUnitPath: slot.getAdUnitPath()
      });
    }
  });
  __iasPET.queue.push({
    adSlots: iasPETSlots,
    dataHandler: function() {
      __iasPET.setTargetingForGPT();
      TKOLIB.adAtfLoad('IAS');
    }
  });
  TKOLIB.adIasLoaded = true;
};
TKOLIB.adIasLoaded = false;
</script>
