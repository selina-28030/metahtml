if (Math.random() * 1000 < 5) {
    pbh_log = function (e) {
        var msg;

        if (typeof e == "string")
            msg = e;
        else
            msg = e.name + ": " + e.message;

        var img = document.createElement('img');
        img.src = '//pbh-network.com/log.gif?' + msg;
        console.log(msg);
    };
} else {
