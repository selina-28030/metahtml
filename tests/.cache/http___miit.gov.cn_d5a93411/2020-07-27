<script>
            jQuery(function($){
                $(".l_tit>a").click(function(){
        var index = $(this).index();
     
     if($(this).hasClass("on")){
      $(this).removeClass("on");
    $(".b_d .l_box").hide();
    }
       else{
      $(this).addClass("on").siblings().removeClass('on');
     $(".b_d .l_box").eq(index).show().siblings().hide();}
                })
            });
        </script>
