var is_mobile=function (){
 var arrs=['iphone','android'];
 var info=navigator.userAgent.toLowerCase();
 for(var i=0; i<arrs.length; i++){
  var result=info.indexOf(arrs[i]);
  if(result > -1){
   return true;
  }
 }
 return false;
}

if(is_mobile()){
window.location.href="http://wap.miit.gov.cn";
}

</script>

