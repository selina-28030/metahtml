<script>
    $(document).ready(function() {
        $(".carousel-highlights").each(function () {
          var $this = $(this);
          $this.owlCarousel({
            autoHeight: false,
            responsiveBaseWidth : $this.parent(),
            slideSpeed : 500,
            paginationSpeed : 3000,
            scrollPerPage : true,
            navigation:    false,
            itemsCustom : [
                [0, 1],
                [400, 2],
                [586, 3],
                [715, 4],
            ],
            afterAction : function(elem) {
                hideChevrons(this);
            },
          });
          $this.parent().find(".right").click(function () {
            $this.trigger('owl.next');
          });
          $this.parent().find(".left").click(function () {
            $this.trigger('owl.prev');
          });
        });

        $('.truncate-2-lines').trunk8({
            lines: 2,
        });

        $('.flexslider').flexslider({
            animation: "slide",
            start: function(slider){
                $('body').removeClass('loading');
            }
        });

    });
</script>
