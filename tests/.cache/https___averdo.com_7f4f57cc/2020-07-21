        <script>
            /** writeEmail */
            function writeEmail(user, host, tld, write) {
                var elem = document.getElementById('email');

                if(elem) {
                    elem.innerHTML = user + '@' + host + '.' + tld;
                }
                else {
                    if(write) {
                        document.write(user + '@' + host + '.' + tld);
                    } else {
                        return (user + '@' + host + '.' + tld);
                    }
                }
            }
            /** writeEmail END */
            /** hide chevrons START */
            function hideChevrons(elem) {
                if (elem.owl.owlItems.length > elem.owl.visibleItems.length) {
                    $(elem.options.responsiveBaseWidth).find(".owl-navi").removeClass("hide");
                }
                else {
                    $(elem.options.responsiveBaseWidth).find(".owl-navi").addClass("hide");
                }
            }
            /** hide chevrons END */
        </script>

