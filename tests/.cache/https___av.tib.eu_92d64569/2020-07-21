	<!-- Matomo -->
	<script type="text/javascript">
/*<![CDATA[*/

			var _paq = _paq || [];
			var video_url = window.location;
			var embedUrl = document.referrer;
			var av_player = "av.tib.eu/player/";
			var videoAttr = "";
			var embedVideo = "";
			var embedVideoAttr = "";
			var eCodeTracked = false;
			var myVideo = document.getElementById("player");
			var website = $(location).attr('hostname');  // e.g. "av.tib.eu";
			var hostname = website;

			// download tracking by 'trackEvent' to get all attributes like flash700 and flash1500 etc.
	 		$(".downloadVideos").click(function() {
	 			//console.log("Video download tracking ...");
// 				alert("Video downloaded!");
				videoAttr = $(this).text();

				if(videoAttr.indexOf("flash700") > -1) videoAttr = "[flash700]";
				if(videoAttr.indexOf("flash1500") > -1) videoAttr = "[flash1500]";

				_paq.push([ 'setDownloadClasses', 'downloadVideos' ]);
// 				_paq.push([ 'trackEvent', 'Video downloads', 'Video-Download: '+video_url, $(this).text()]);
				_paq.push([ 'trackEvent', 'Video downloads', 'Download: '+video_url, videoAttr +' '+video_url ]);
				//console.log("Video download tracking done!");
			});

			// Log how much time was spent during the visit
			var start = new Date();
			jQuery(window).on('beforeunload unload', function(){
				var end = new Date();
				var timeSpent = end - start;
				var sec = parseInt(timeSpent / 1000);
				var min = parseInt(sec / 60);
				sec = sec % 60;
				sec = sec < 10 ? "0" + sec : sec;
				var hrs = parseInt(min / 60);
				min = min % 60;
				min = min < 10 ? "0" + min : min;
				var days = parseInt(hrs / 24);
				hrs = hrs % 24;
				hrs += days * 24;
				_paq.push([ 'trackEvent', 'Time spent', hrs + ":" + min + ":" + sec]);
			});

			// Log if "Recognized Entities" tab was activated
			jQuery('#aggregated').on('click', function() {
				if (!jQuery(this).hasClass('active')) {
					_paq.push([ 'trackEvent', 'Recognized entities', 'activated']);
				}
			});

			// Log if "Speech transcript" tab was activated
			jQuery('#transcript').on('click', function() {
				if (!jQuery(this).hasClass('active')) {
					_paq.push([ 'trackEvent', 'Speech transcript', 'activated']);
				}
			});

			// Log if an entry from speech transcript was clicked
			jQuery('.timebased-row').on('click', function() {
				if (jQuery(this).parent().is('#mCSB_2_container')) {
					var timedLink = jQuery(this).children('.timedLink').text();
					_paq.push([ 'trackEvent', 'Speech transcript', 'Transcript entry clicked at: ' + timedLink]);
				}
			});

			// Log if a recognized entity was clicked
			var entity = jQuery('.entity.asr, .entity.vcd, .entity.ocr');
			entity.on('click', function() {
				var entityValue = jQuery(this)[0].textContent;
				var sources = jQuery(this).attr('sources');
				//console.log(entityValue);console.log(sources);
				_paq.push([ 'trackEvent', 'Recognized entity clicked', sources.toUpperCase() + ': ' + entityValue]);
			});

			// Log if the search field was clicked
			jQuery('#temporalSearchInput').on('click', function() {
				_paq.push([ 'trackEvent', 'Recognizied entities', 'Search field used']);
			});

			// Log if an entity label (category) was (de)selected
			var entitiesLabel = jQuery('#asr-label, #vcd-label, #ocr-label');
			entitiesLabel.on('click', function() {
				var labelValue = jQuery(this)[0].textContent;
				_paq.push([ 'trackEvent', 'Recognized entity label clicked', labelValue]);
			});

			// Log if language was clicked
			jQuery('.metanav-ul > .metanav-li:nth-of-type(3) > a').on('click', function() {
				var nextLang = jQuery('.metanav-ul > .metanav-li:nth-of-type(3) > a').text();
				var currentLang = '';
				nextLang==="Deutsch" ? currentLang = "English" : currentLang = "Deutsch";
				// Timeout needs to be set, otherwise it collies with unload/beforeunload
				setTimeout(function(){ _paq.push([ 'trackEvent', 'Language changed: ', currentLang + ' > ' + nextLang]); }, 200);
			});

			// tracking video by embed code on other pages
			$( function() {
				if ( $("#player").hasClass("mejs__tib") ) {
// 					console.log(video_url);
// 					hostname = $(location).attr('hostname');

 					// check for embedded video
					if( $("a").is("#embeddedPlayerLink")) {
						embedVideo = $("#embeddedPlayerLink").attr("href");
						overlay = $(".mejs__overlay .mejs__layer .mejs__overlay-play").attr("class");
						hostname = "embedVideo";

						// tracking when autoplay is active
						if(embedUrl.indexOf(av_player) == -1) {
// 							console.log(embedUrl + " : " + embedVideo);

							if ( eCodeTracking("player", embedUrl, embedVideo, eCodeTracked) == true ) {
								eCodeTracked = true;
								embedVideoAttr = "(auto)";
								ma_tracker();
							} else eCodeTracked == false;

// 							// tracking when play overlayed button is clicked
							$(".mejs__overlay-play .mejs__overlay-button").click(function() {
								console.log(".mejs__overlay-button");
							});

							// tracking when play button is clicked
							$(".mejs__play button").click(function() {
								console.log(".mejs__play button- Tracked:" + eCodeTracked);

								if (eCodeTracked == false) {
									// invert paused status to true because after click the video is already playing!
									if (myVideo.paused == true) {
										_paq.push([ 'trackEvent', 'Embed Playing Videos', 'Embed-Video ' + embedVideoAttr + 'plays on: '+embedUrl, embedVideo ]);
							    		eCodeTracked = true;
							    		ma_tracker();
									}
						    	}
							});

							$("body").click(function() {
// 								alert("Clicked! - Tracked:" + eCodeTracked);
								if (eCodeTracked == false) {
									if (myVideo.paused == false) {
										_paq.push([ 'trackEvent', 'Embed Playing Videos', 'Embed-Video ' + embedVideoAttr + 'plays on: '+embedUrl, embedVideo ]);
							    		eCodeTracked = true;
							    		ma_tracker();
									}
						    	}
							});
						}
					}
				}


				// only track when video is not embedded
// 				alert(website + " : " + hostname);
				if(website == hostname) {
					ma_tracker();
				}
			});

		/**** function implementation ****/
		function ma_tracker() {
			// paste mamoto tracking code here

			// _paq variable removed, because it was already initialized on the top of this script!!!
			//var _paq = _paq || [];
			/* tracker methods like "setCustomDimension" should be called before "trackPageView" */
			_paq.push([ 'trackPageView' ]);
			_paq.push([ 'enableLinkTracking' ]);

			(function() {
				var u = "//support.tib.eu/matomoav/";
				_paq.push([ 'setTrackerUrl', u + 'piwik.php' ]);
				_paq.push([ 'setSiteId', '1' ]);
				var d = document, g = d.createElement('script'), s = d.getElementsByTagName('script')[0];
				g.type = 'text/javascript'; g.async = true; g.defer = true; g.src = u + 'piwik.js'; s.parentNode.insertBefore(g, s);
			})();

		}

		function eCodeTracking(videoPlayer, embedUrl, embedVideo, trackedStatus) {
			var myVideo = document.getElementById(videoPlayer);
			var tracked = trackedStatus;

			if (tracked == false) {
				if (myVideo.paused == false) {
					console.log("Yay! Video is playing!");
					if (embedUrl != "") {
						_paq.push([
								'trackEvent',
								'Embed Playing Videos',
								'Embed-Video ' + embedVideoAttr + 'plays on: '
										+ embedUrl, embedVideo ]);
						tracked = true;
					}
				} else {
					console.log("Video is paused!");
					tracked = false;
				}
			}

			return tracked;
		}
	
/*]]>*/
</script>
	<!-- End Matomo Code -->

