$.get('https://ipapi.co/json/', function(response){
  $.get('https://api.openweathermap.org/data/2.5/weather?lat=' + response.latitude + '&lon=' + response.longitude + '&units=metric&appid=ba07ec7804dc2bdf6004649866f2c6c5', function(wResponse){

    $( "#displayit" ).append(wResponse.name+" <img src='https://openweathermap.org/img/w/"+wResponse.weather['0'].icon+".png' style='width:20px;height:20px;'> "+wResponse.main.temp+'&deg;C' );
  })

		var d = new Date();
    $( "#displaydate" ).append(d.toLocaleString('en-US', {
    		dateStyle:"full",
    		timeStyle:'medium',
    		hour:'numeric',
            minute:'numeric',
            second:'numeric',
            hour12:true}));
	
	})
