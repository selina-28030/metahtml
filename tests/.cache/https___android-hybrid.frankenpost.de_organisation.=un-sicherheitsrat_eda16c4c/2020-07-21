  <script type="text/javascript">

	var mmenuOpen = false;

    fcmsJs.load('extlib_jquerymmenu').onLoad(function() {

      jQuery('#mymenu').mmenu({
        "slidingSubmenus": false,
        "iconPanels": true,
        "navbar": false,
        "hooks": {
            "open:finish": function() {
               
               mmenuOpen = true;
            },
            "close:finish": function() {
               
               mmenuOpen = false;
            }
         }
      });

    });

    /**
   * Callbackfunktion für die Android-App. Die Funktion öffnet/schließt aus der App heraus das Menü. Definiert in der App-Config
   */
    function toggleMmenu(){

      var menuApi = jQuery("#mymenu").data( "mmenu" );
      if (menuApi !== null
          && typeof menuApi !== 'undefined')
      {
        if(mmenuOpen === false){
          menuApi.open();
        } else {
        	menuApi.close();
        }
      }

    }
  </script>


