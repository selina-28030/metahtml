		if(NowChannel1 == undefined || NowChannel1 == "" ){
			NowChannel1 = $("div.station").find("a:last-child").html();//æ¦è§ä¸­ä¸ºstation
		}

        $(".menu li").each(function(){
             var channelDesc=$(this).find("a").html().trim();
             if(channelDesc==NowChannel1){
                 $(this).addClass("now").siblings().removeClass("now");
             }
        });
        //å·¦ä¾§æ ç®ç¹å»é«äº®
        $(".menu li").click(function(){
            $(this).addClass("now").siblings().removeClass("now");
        });
		
		//å¯¼èªæ ä¾§æ ç®é«äº®ç»æ
        	 
 });
function checkform() {
        	 
        	 var iptSword = $.trim($('#iptSword').val());
        		if(iptSword == "" || iptSword == "è¯·è¾å¥å³é®å­" || !isValid(iptSword)){
        		  alert("è¯·è¾å¥å³é®è¯ååè¿è¡æäº¤ã");
        	      return false;
        		}
        		$('input[name="keyword"]').val(encodeURI(iptSword));
        		$('form[name="searchform"]').submit();
        		return false;
        	
        	 }
