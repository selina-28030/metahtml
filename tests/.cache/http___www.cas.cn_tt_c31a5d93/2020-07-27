<script type="text/javascript">
$(function(){
        	 
        	 
	  //å¯¼èªæ ä¸»æ ç®é«äº®å¼å§
		var location = $("div.station2").find("a").eq(1).html();//ç»è§ä¸­ä¸ºstation2
		if(location == undefined || location == "" ){
			location = $("div.station").find("a").eq(1).html();//æ¦è§ä¸­ä¸ºstation
		}
        //å¯¼èªä¸»æ ç®é«äº®
        $("#nav li").each(function(){
            var channelDesc=$(this).find("a").html();
            if(channelDesc == location){
                 $(this).addClass("now").siblings().removeClass("now");
            }
        });
        $(".pad_nav li").each(function(){
            var channelDesc=$(this).find("a").html();
            if(channelDesc == location){
                 $(this).addClass("cur").siblings().removeClass("cur");
				setSwiperNav($(".pad_nav li").index($(".pad_nav li.cur")));

            }
        });
         setSwiperNav(0);
		//å¯¼èªæ ä¸»æ ç®é«äº®ç»æ
		
		//å¯¼èªæ ä¾§æ ç®é«äº®å¼å§
