						var is_member_login = 0, historyId = false;
						$(function(){
							$('.search-mask-header .close').click(function(){
								$('body').css("overflow-y",'auto');
								$('.portal').hide();
							});

							$('.searchIcon').click(function(){
								var val = $('.search-mask-search-view .searchInput').val();;
								location.href = '/search?p=' + encodeURIComponent(val);
							});

							$(".search-button").click(function(){
								$('body').css("overflow-y",'hidden');
								$('.portal').show();
								$('.search-mask-search-view .searchInput').val('');
								$('.search-mask-search-view .searchInput').focus();
							});

							$(window).scroll(function(){
								if($(window).scrollTop() > 60 ){
									$('#page-header-bottom').hide();
									$('#page-header-top').addClass('fixed');
									$('.contentWrap').addClass('fixed');
								}else {
									$('#page-header-bottom').show();
									$('#page-header-top').removeClass('fixed');
									$('.contentWrap').removeClass('fixed');
								}
							})
						})
					</script>
