<script>
	$(function(){
		function getCookie(name){ 
		    var arr,reg=new RegExp("(^| )"+name+"=([^;]*)(;|$)");
		    if(arr=document.cookie.match(reg))
		 
		        return unescape(arr[2]); 
		    else 
		        return null; 
		} 
		if(getCookie('iyiou_www_new_wxLoginType')){
			$('.weixinloginedAlert,#background').addClass('active');
			$('.wxNickname').html(decodeURI(getCookie('iyiou_www_new_nickname')));
			$('.wxUserPic').children('img').attr('src',getCookie('iyiou_www_new_headimgurl'))
		}
		var date=new Date();
		date.setTime(date.getTime()-10000);
		document.cookie="iyiou_www_new_wxLoginType=0; expires="+date.toGMTString(); 
	})
</script>
