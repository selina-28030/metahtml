    <script type="text/javascript">
        var Industry="yiyaodianshang"||'0';
        $('.footer_header').addClass('other');

        var allW = $(window).width(),boxW = $('.box').width();
        window.onscroll = function(e){
        	if($(window).scrollTop() > 60){
        		$('#page-header-top').removeClass('fixed');
        		$('#page-header-bottom').show();
        	}
        	if($(window).scrollTop() > 60){
        		$('.secondLevelWrap').css({
        			'position': 'fixed',
        			'left': (allW-boxW)/2+'px',
        			'top': '60px'
        		});
        	}else {
        		$('.secondLevelWrap').css({
        			'position': 'absolute',
    			    'left': '-140px',
    			    'top': 0
        		});
        	}
    		if($(document).height() - $(window).height() - $('.page_footer').height() + $(window).height()-$('.secondLevelWrap').height() - 65  < $(window).scrollTop()){
        		$('.secondLevelWrap').css({
        			'position': 'absolute',
        			'bottom': '40px',
        			'top': 'inherit',
        			'left': '-140px'
        		});
        	}
        }

        $(function(){
        	var urlObj = [
	        	{
	        		name: 'jinrong',
	        		title: '保险业'
	        	},
	        	{
	        		name: 'lingshou',
	        		title: '零售业'
	        	},
