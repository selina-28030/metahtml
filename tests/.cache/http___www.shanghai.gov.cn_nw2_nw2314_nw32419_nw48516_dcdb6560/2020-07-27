<script type="text/javascript">
    function zoom_image(obj) {
        if (obj.hasClass('photoBox')) {
            var load = obj.find('.loadingBox');
            load.show();
            var img = obj.next().find('img');
            if (img.attr('src') == 'about:blank') {
                img.attr('src', obj.find('img').attr('src').replace('m.', 'l.'));
                img.load(function () {
                    obj.hide();
                    obj.next().show();
                });
            } else {
                obj.hide();
                obj.next().show();
            }
        } else {
            obj.hide();
            obj.prev().show();
            obj.prev().find('.loadingBox').hide();
        }

        sethash();
    }
</script>
<script type="text/javascript">
    $(function ()
    {
        $(".tabbox li").hover(function ()
        {
            //»ñÈ¡µã»÷µÄÔªËØ¸øÆäÌí¼ÓÑùÊ½£¬½²ÆäÐÖµÜÔªËØµÄÑùÊ½ÒÆ³ý
            $(this).addClass("active").siblings().removeClass("active");
            //»ñÈ¡Ñ¡ÖÐÔªËØµÄÏÂ±ê
            var index = $(this).index();
            $(this).parent().siblings().children().eq(index).addClass("active")
                .siblings().removeClass("active");
        });
    });

</script>
