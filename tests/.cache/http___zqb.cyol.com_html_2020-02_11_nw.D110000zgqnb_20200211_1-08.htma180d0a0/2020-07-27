<script>
img_filter();

//å¾ææ··æå¾çå»é
/**
æ­¤æ¹æ³éè¦æ ¹æ®å®éæç« æ¨¡æ¿è¿è¡è°æ´ï¼å¦åæ æ³çæ
*/
function img_filter() {
	var imgs = document.getElementById("reslist").getElementsByTagName("img");
	//æ¨¡æ¿ä¸æ­£æåºåçdivçidä¸ºarticleTextï¼åèå·ä½æ¨¡æ¿è°æ´ï¼
	var contentimglist= document.getElementById("articleContent").getElementsByTagName("img");    
	var len = contentimglist.length;
	var imglen = imgs.length;
	var count = 0;
	var delobjs = new Array();
	outer:
	for (var i = 0; i < imgs.length; i++) {
		for (var j = 0; j < contentimglist.length; j++) {
			if(imgs[i].src == contentimglist[j].src){			  
				delobjs.push(imgs[i]);
				count++;
				if(count>=len){
					break outer;
				}
			}
		}
	}

	//å é¤éå¤åç´ 
	for(var i=0;i<delobjs.length;i++){
		//æ ¹æ®æ¨¡æ¿ç´ ææ¥æ¾ç´ æå¾çæå¨çåç´ ï¼éè¦æ ¹æ®å®éæ¨¡æ¿è¿è¡è°æ´ï¼
		var imgObj = delobjs[i];
		var delEle = imgObj.parentNode.parentNode.parentNode.parentNode;
		//imgs[i].parentNode.remove(imgs[i]);
		delEle.parentNode.removeChild(delEle);
	}
	
	//æ£æ¥ç´ æåè¡¨åºåæ¯å¦è¿æå¾çï¼å¦ææ²¡æåå æè¯¥åºåæå¤å±å ä½åç´ 
	//æ¨¡æ¿ä¸ç´ æåè¡¨åºåçæå¤å±å ä½tableçidä¸ºtableInfoï¼åèå·ä½æ¨¡æ¿è°æ´ï¼
	//æ­¤æ¨¡æ¿å°±æ¯åæ¾å°å¾ççtableï¼ç¶åæ¾å°tableçåä¸¤ä¸ªåå¼åç´ ï¼ææ¯å ä½åç´ ï¼ç¶åå é¤è¯¥åç´ 
	var resimglist= document.getElementById("reslist").getElementsByTagName("img");
	if(resimglist.length<1){
		var obj = document.getElementById("reslist");//$("#tableInfo");
		//var delobj=obj.previousSibling.previousSibling;
		delobj=obj;
		delobj.parentNode.removeChild(delobj);
	}
}
</script>
