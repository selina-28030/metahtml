	var userAgent = window.navigator.userAgent.toLowerCase();
	if(userAgent.indexOf('safari') != -1) {
		var frame;
		frame = document.createElement('iframe');
		frame.src = 'https://jen.jiji.com/cookie.html';
		frame.style.display = 'none';
		frame.id = 'itp';
