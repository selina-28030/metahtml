var topArray = [
     { "link": "https://news.mynavi.jp/tag/business_terms/", "img": "https://news.mynavi.jp/publicimg/banner/name/3167/banner.jpg"},
     { "link": "https://news.mynavi.jp/tag/drama/", "img": "https://news.mynavi.jp/publicimg/banner/name/3146/banner.jpg"}, 
     { "link": "https://news.mynavi.jp/tag/iphone_why/", "img": "https://news.mynavi.jp/publicimg/banner/name/3144/banner.jpg"}, 
     { "link": "https://news.mynavi.jp/top/business/technology/aerospace/", "img": "https://news.mynavi.jp/publicimg/banner/name/3163/banner.jpg"},
     { "link": "https://news.mynavi.jp/top/entertainment/hobby/tokusatu/", "img": "https://news.mynavi.jp/publicimg/banner/name/3139/banner.jpg"},
     { "link": "https://news.mynavi.jp/features/list/enterprise", "img": "https://news.mynavi.jp/publicimg/banner/name/3166/banner.jpg"},
     { "link": "https://news.mynavi.jp/top/kurashi/tokyo2020/interview/", "img": "https://news.mynavi.jp/publicimg/banner/name/3140/banner.jpg"},
     { "link": "https://news.mynavi.jp/series/win10tips", "img": "https://news.mynavi.jp/publicimg/banner/name/3155/banner.jpg"},
     { "link": "https://news.mynavi.jp/list/headline/digital/kaden/camera/article_type/review/", "img": "https://news.mynavi.jp/publicimg/banner/name/3150/banner.jpg"},
     ];


const shuffle = ([...array]) => {
  for (let i = array.length - 1; i >= 0; i--) {
    const j = Math.floor(Math.random() * (i + 1));
    [array[i], array[j]] = [array[j], array[i]];
  }
  return array;
}

topArray = shuffle(topArray);
$('#rotationBannerTop li').each(function(i, val){
      $(val).children('a').attr('href', topArray[i]['link']).children('img').attr('data-delay', topArray[i]['img']);
});
</script>
