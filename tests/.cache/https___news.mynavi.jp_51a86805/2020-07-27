<script>
  if (typeof news_vars !== "undefined") {
    var googletag = googletag || {};
    googletag.cmd = googletag.cmd || [];

    googletag.cmd.push(function () {
      googletag.pubads().disableInitialLoad();
      googletag.pubads().collapseEmptyDivs();
      googletag.pubads().enableSingleRequest();
      googletag.enableServices();
    });

    if(!window.BiddingManager){window.BiddingManager={_env:null,que:[],init:function(a){this._env=a}}};

    // JSONの読み込みを開始
    if(location.hostname.match(/(stg\.|dev\d{2}\.)news.mynavi.jp|127.0.0.1/)) {
      BiddingManager.init({ configUrl: 'https://stg.ad.woman.mynavi.jp/json/3cdeab9f/env.json' });
    } else {
      BiddingManager.init({ configUrl: 'https://ad.woman.mynavi.jp/json/3cdeab9f/env.json' });
    }

    BiddingManager.que.push(function(bm) {
      // JSONの内容を適用するために必要
      bm.setConfig();

      // JSONで定義された広告情報を有効化前に改変する
      bm.mapSlot(function(slot) {
        // すべてのslotのKeyValueに動的な値を追加
        slot.keyValues = slot.keyValues || {};
        if (slot.keyValues) {
          slot.keyValues.ad_group = Adomik.randomAdGroup();
          slot.keyValues.ad_h = (new Date).getUTCHours().toString();
        }

        // 必ず改変後のslotを返す
        return slot;
      });

      //1次リリース用（2次リリース時に削除する）
      // no-ad
      var is_no_ad = false;
      if(!news_vars.ad && !news_vars.brandgate && !news_vars.categoryjack && news_vars.ptype && news_vars.ch != "hisamitsu")is_no_ad = true;
      // article
      var is_article = false;
      if(news_vars.ptype !== "undefined" && !news_vars.ptype.match(/^top.*/))is_article = true;
      // image
      var is_large_image = false;
      if(news_vars.displayForm !== "undefined" &&  news_vars.displayForm == "image")is_large_image = true;
      // top
      var is_top = false;
      if(news_vars.ptype == "top")is_top = true;
      // ch_top
      var is_topch = false;
      if(news_vars.ptype == "top-ch")is_topch = true;
      // cat_top
      var is_topcat = false;
      if(news_vars.ptype == "top-cat")is_topcat = true;

      var ids = []

      if (!is_top && !is_large_image && is_no_ad && document.referrer != location.href && !document.referrer.match(/(news|headlines)\.yahoo\.co\.jp/)) {
        // mn.welcome
        ids.push('div-gpt-ad-1479185899023-20');
      }
      if (!is_topch && !is_top && (is_no_ad || news_vars.categoryjack && !news_vars.ad || news_vars.cat == "tokyo2020" || news_vars.cat == "cashless")) {
        // mn.billboard
        ids.push('div-gpt-ad-1466143984602-0');
      }
      if (!is_topcat && !is_topch && !is_top && !is_large_image && (is_no_ad || news_vars.categoryjack && !news_vars.ad && is_article)) {
        // mn.inread
        ids.push('div-gpt-ad-1466143984602-1');
      }
      if (!is_top) {
        // mn.reca
        ids.push('div-gpt-ad-1479185899023-14');
      }else{
        // mn.reca-top
        ids.push('div-gpt-ad-1593574342922-0');
      }
      if (!is_top) {
        // mn.recb
        ids.push('div-gpt-ad-1479185899023-16');
      }
      if (is_article && !is_large_image) {
        // mn.recl
        ids.push('div-gpt-ad-1479185899023-18');
        // mn.recr
        ids.push('div-gpt-ad-1479185899023-19');
      }
      if (!is_topch && !is_top && is_no_ad) {
        // mn.overlay
        ids.push('div-gpt-ad-1507249628623-0');
      }
      if (!is_top && !is_large_image && is_article && (is_no_ad || news_vars.brandgate || news_vars.categoryjack && !news_vars.ad)) {
        // mn.contbottom1
        ids.push('div-gpt-ad-1479185899023-0');
      }
      if (!is_topch && !is_top && !is_large_image) {
        // mn.recc
        ids.push('div-gpt-ad-1485738116644-0');
      }
      if (!is_top && !is_large_image && is_no_ad) {
        // mn.gateside
        ids.push('div-gpt-ad-1479185899023-6');
      }
      if (!is_topch && (is_no_ad || news_vars.brandgate || news_vars.categoryjack && !news_vars.ad)) {
        // mn.footer
        ids.push('div-gpt-ad-1479185899023-4');
      }
      if (!is_topch && !is_top && !is_large_image && (is_no_ad || news_vars.brandgate || news_vars.categoryjack && !news_vars.ad)) {
        // mn.headline1
        ids.push('div-gpt-ad-1479185899023-7');
        // mn.headline2
        ids.push('div-gpt-ad-1479185899023-8');
        // mn.headline3
        ids.push('div-gpt-ad-1479185899023-9');
        // mn.headline4
        ids.push('div-gpt-ad-1479185899023-10');
      }
      if (!is_topch && !is_top && (is_no_ad || news_vars.brandgate || news_vars.categoryjack && !news_vars.ad)) {
        // mn.rank1
        ids.push('div-gpt-ad-1479185899023-12');
        // mn.rank2
        ids.push('div-gpt-ad-1479185899023-15');
      }
      if (!is_topch && !is_top && !is_large_image && is_no_ad && is_article) {
        // mn.kanren1
        ids.push('div-gpt-ad-1535073804592-0');
      }
      if (!is_large_image && is_article && (is_no_ad || news_vars.brandgate || news_vars.categoryjack && !news_vars.ad)) {
        // mn.contbottom2
        ids.push('div-gpt-ad-1479185899023-1');
      }
      // スロットの有効化
      bm.activateSlots(ids);

      var gnr;
      var ch;
      var cat;

      if (typeof news_vars.genre == "undefined" || news_vars.genre == "") {
        gnr = "top";
        ch = "top";
        cat = "";
      } else {
        if (typeof news_vars.ch == "undefined" || news_vars.ch == "") {
          ch = "top";
          cat = "";
        } else {
          if (typeof news_vars.cat == "undefined" || news_vars.cat == "") {
            cat = "top";
          } else {
            cat = news_vars.cat.replace(/\-|\_/,"");
          }
          ch = news_vars.ch.replace(/\-|\_/,"");
        }
        gnr = news_vars.genre.replace(/\-|\_/,"");
      }

      // ページレベルでKeyValueを追加
      bm.setPageKeyValue({
        gnr: 'mn.' + gnr,
        ch : 'mn.' + ch,
        cat: 'mn.' + ch + "." + cat
      });

      if (news_vars.brandgate || news_vars.categoryjack || news_vars.ch == "hisamitsu") {
        bm.setPageKeyValue({ mp: 'yes' });
      }

      if(location.hostname.match(/(stg\.|dev\d{2}\.)news.mynavi.jp/)) {
        bm.setPageKeyValue({ sitetype: 'test' });
      }

      if(typeof news_vars.ptype !== "undefined" && news_vars.ptype.match(/^top.*/)) {
        bm.setPageKeyValue({ mnpagetype: news_vars.ptype });
      } else if(typeof news_vars.displayForm !== "undefined" && news_vars.displayForm.match(/^image$/)) {
        bm.setPageKeyValue({ mnpagetype: news_vars.displayForm });
      }

      if(news_vars.analyticsTag instanceof Array && typeof news_vars.analyticsTag[0] !== "undefined") {
        var tag = news_vars.analyticsTag || '';
        var mntag_array = tag.map(function(e) {
          return e.replace(/\-|\_/,"");
        });
        bm.setPageKeyValue({ mntag: mntag_array });
      }else {
        bm.setPageKeyValue({ mntag: 'none' });
      }

      // keywords
      if (typeof news_vars.tags !== "undefined" && news_vars.tags instanceof Array) {
        bm.setPageKeyValue({ mnkeyword: news_vars.tags });
      }

      // ads
      var exclude_cat_filter = ['mn.entertainment.geinou','mn.hobby.model','mn.life.ed','mn.life.workstyle'];
      if (location.href.match(/^.*news\.mynavi\.jp\/((tag|ranking)\/|(top|list\/newsworthy|list\/headline|list\/kikaku|ranking)\/entertainment\/|freeword)/)) {
        bm.setPageKeyValue({ ads: 'ng' });
      // TODO テスト
      } else if (exclude_cat_filter.indexOf('mn.' + ch + "." + cat) > -1) {
        if(news_vars.adParameter instanceof Array && /ok/.test(news_vars.adParameter[0])) {
          bm.setPageKeyValue({ ads: 'ok' });
        } else {
          bm.setPageKeyValue({ ads: 'ng' });
        }
      } else {
        if(news_vars.adParameter instanceof Array && /ng/.test(news_vars.adParameter[0])) {
          bm.setPageKeyValue({ ads: 'ng' });
        } else {
          bm.setPageKeyValue({ ads: 'ok' });
        }
      }

      // オークションを開始
      bm.startBidding();
    });
  }
</script>


  
</head>

