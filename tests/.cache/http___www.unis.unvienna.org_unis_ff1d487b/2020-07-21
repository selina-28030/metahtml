                   function pagerBuilder(index, DOMelement) {
                        var aTag = '<a href="&#039; + DOMelement + &#039;">' + DOMelement + '</a>'
                        $('#rotating-image-pager').append('<li>' + DOMelement.innerHTML + '</li>');
                   }

                   function pagerLinkActivator(pagerId,currSlideIndex,className) {
                        $(pagerId).find('li').removeClass(className)
                        .filter('li:eq('+currSlideIndex+')').addClass(className)
                        .click(function(){document.location($('a',this).attr('href'))});
                   }                   
