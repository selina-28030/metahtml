	document.querySelectorAll( '.js-headline-testing-preload' ).forEach(function(articleElement) {
		var randomArticle = testData[ Math.floor( Math.random() * testData.length ) ];
		var randomVariant = randomArticle.variants[ Math.floor( Math.random() * randomArticle.variants.length ) ];

		// All data is sanitized prior to display.
		var articleHeadline;
		if ( -1 !== articleElement.className.indexOf('dont-miss') ) {
			articleElement.querySelector('a').setAttribute('href', randomVariant.permalink);
			articleElement.querySelector('a .title').innerHTML = randomVariant.title;
		} else if ( -1 !== articleElement.className.indexOf('trending') ) {
			articleElement.querySelector('a').setAttribute('href', randomVariant.permalink);
			articleElement.querySelector( '.headline' ).innerHTML = randomVariant.title;
			if ( articleElement.querySelector( 'img' ) ) {
				articleElement.querySelector( 'img' ).setAttribute( 'src', randomArticle.image_src );
			}
		} else {
			articleHeadline = null !== articleElement.querySelector( '.headline a' ) ? articleElement.querySelector( '.headline a' ) : articleElement.querySelector( '.entry-title a' );
			articleHeadline.setAttribute( 'href', randomVariant.permalink );
			articleHeadline.innerHTML = randomVariant.title;
		}

		var featuredImage = articleElement.querySelector( '.featured-image a' );
		if ( null !== featuredImage ) {
			featuredImage.setAttribute( 'href', randomVariant.permalink );
			featuredImage.innerHTML = randomArticle.image_html;
		}

		if ( null !== articleElement.querySelector( '.index-entry-meta-top' ) ) {
			articleElement.querySelector( '.index-entry-meta-top' ).innerHTML = randomArticle.entry_meta_top;
		}
		if ( null !== articleElement.querySelector( '.inner .primary-category' ) ) {
			articleElement.querySelector( '.inner .primary-category' ).innerHTML = randomArticle.channel_name;
		}
		if ( null !== articleElement.querySelector( '.entry-meta-mobile' ) ) {
			articleElement.querySelector( '.entry-meta-mobile' ).innerHTML = randomArticle.entry_meta_mobile;
		}
		if ( null !== articleElement.querySelector( '.entry-content' ) ) {
			articleElement.querySelector( '.entry-content' ).innerHTML = randomArticle.excerpt;
		}

		var classes = randomArticle.classes + ' ' + randomVariant.classes;
		if ( -1 !== articleElement.className.indexOf('dont-miss') ) {
			classes += ' dont-miss';
		}
		articleElement.setAttribute( 'class', classes );

		var reportEvent = function( key, value ) {
			var xhr = new XMLHttpRequest();
			xhr.open('POST', 'https://a119e778.bgr.com/api/write', true);
			xhr.setRequestHeader('Content-Type', 'application/json');
			xhr.send(JSON.stringify({
				key: key,
				value: value,
			}));
		};

		reportEvent(randomVariant.classes, 'inview');

		var linkElement = document.querySelector( '.' + randomVariant.classes + ' a' );
		if ( null !== linkElement ) {
			linkElement.addEventListener('click', function(){
				reportEvent(randomVariant.classes, 'click');
			});
		}
	});

