(function(){
	window.pmc.hooks.add_filter( 'pmc-google-analytics-tracking-events', function( event, element ){
		if ( 'undefined' !== typeof event.eventAction &&
			'click' === event.eventAction &&
			'undefined' !== typeof event.eventLabel &&
			-1 !== event.eventLabel.indexOf( 'deals-link' ) ) {
			window.fbq( 'track', 'AddToCart' );
		}
		if ( 'undefined' !== typeof event.eventAction &&
			'click' === event.eventAction &&
			'undefined' !== typeof event.eventLabel &&
			-1 !== event.eventLabel.indexOf( 'amazon' ) ) {
			window.fbq( 'track', 'InitiateCheckout' );
		}
		return event;
	});
}());
