				document.getElementById("img" + img_temp).style.display = "block";
				document.getElementById("img_text" + img_temp).style.display = "block";
				
			}
			
			else
			{
				document.getElementById("img" + img_temp).style.display = "none";
				document.getElementById("img_text" + img_temp).style.display = "none";
				document.getElementById("img_icon" + img_temp).style.display = "none";
				img_temp = img_temp-1;
				document.getElementById("img" + img_temp).style.display = "block";
				document.getElementById("img_text" + img_temp).style.display = "block";
				document.getElementById("img_icon" + img_temp).style.display = "block";
			
			}
		}
	
	function right_button()
		{
	
			if  ( img_temp == length_img) {		
			
				document.getElementById("img" + img_temp).style.display = "none";
				document.getElementById("img_text" + img_temp).style.display = "none";
				document.getElementById("img_icon" + img_temp).style.display = "none";
				img_temp = 0;
				document.getElementById("img" + img_temp).style.display = "block";
				document.getElementById("img_text" + img_temp).style.display = "block";
				document.getElementById("img_icon" + img_temp).style.display = "block";
				
			}
			else
