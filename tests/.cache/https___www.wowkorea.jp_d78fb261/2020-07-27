	var length_img;
	var Stop_switch;
	
	length_img = 4;
	img_temp = 0;
	Stop_switch = 0;
	
	function left_button()
		{

			if  (img_temp == 0) {
				document.getElementById("img" + img_temp).style.display = "none";
				document.getElementById("img_text" + img_temp).style.display = "none";
				document.getElementById("img_icon" + img_temp).style.display = "none";
