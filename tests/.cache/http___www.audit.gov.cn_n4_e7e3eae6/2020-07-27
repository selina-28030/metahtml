<script type="text/javascript">
function setTabln(name,cursel,n){
for(i=2;i<=n;i++){   // åi = 1ï¼âæºå³âæ ç®å·²å é¤ï¼åç»­å¢å æ ç®å¨id="dc153" åé¢æ·»å 
var menu=document.getElementById(name+i);
var con=document.getElementById("con_"+name+"_"+i);
menu.className=i==cursel?"white14_24":"black14_24";
con.style.display=i==cursel?"block":"none";
}
}
</script>

