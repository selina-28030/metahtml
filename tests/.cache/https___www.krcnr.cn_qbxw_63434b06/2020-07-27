<script>    
    var pageHtml = function(count, currentIndex, pageName, extName){
		var ele = document.getElementById("page");
        if(count == null || count<=1){
            return;
        }

        var str="";
        if(count !== 1){//总数不等于1
            if(count === (currentIndex+1) ){//当前页就是最后一页
                if(currentIndex == 1){
                    str='<a href="' + pageName + '.' + extName + '" class="preBtn"></a><span class="num">2/2</span><span class="go" id="go">Go</span><input type="text" placeholder="跳转至" />';
                }else{
                    str='<a href="' + pageName + '_' + (currentIndex - 1) + '.' + extName + '" class="preBtn"></a><span class="num">' + count + '/'+ count + '</span><span class="go" id="go">Go</span><input type="text" placeholder="跳转至" />';
                }
            }else{//当前页不是最后一页
                switch (currentIndex){
                    case 0:
                        str='<span class="num">1/' + count + '</span><a href="' + pageName + '_' + (currentIndex + 1) + '.' + extName + '" class="nextBtn"></a><span class="go" id="go">Go</span><input type="text" placeholder="跳转至" />';
                        break;
                    case 1:
                        str='<a href="' + pageName + '.' + extName + '" class="preBtn"></a><span class="num">2/' + count + '</span><a href="' + pageName + '_' + (currentIndex + 1) + '.' + extName + '" class="nextBtn"></a><span class="go" id="go">Go</span><input type="text" placeholder="跳转至" />';
                        break;
                    default:
                        str='<a href="' + pageName + '_' + (currentIndex - 1) + '.' + extName + '" class="preBtn"></a><span class="num">' + (currentIndex+1) + '/' + count + '</span><a href="' + pageName + '_' + (currentIndex + 1) + '.' + extName + '" class="nextBtn"></a><span class="go" id="go">Go</span><input type="text" placeholder="跳转至" />';
                }
            }
        }

        /*str+='<div class="paginationgo">'+
                '<input type="text" placeholder="跳转至" /><button id="go">GO</button>'+
             '</div>';*/

        ele.innerHTML = str;
    }
	pageHtml(20,0,"index","html");
	$(document).on("click",function(evt){
		var target = $(evt.target);
        if(target.attr('id') === 'go'){
			var num = target.next().val();
            if( !isNaN(num) ){
				if(num < 1 && num > (20-1)){
					alert("请输入范围内的数字");
                }else{
					if(num == 1){
						window.location = 'index.html';
                    }else{
                        window.location = 'index_' + (num-1) + '.html';
                    }
                }
            }else{
				alert("请输入数字");
            }
        }
    });
</script>
