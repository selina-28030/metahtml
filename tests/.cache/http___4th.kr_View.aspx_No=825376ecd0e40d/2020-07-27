        function FireEnterKey(e, callback) {
            if (e.keyCode == 13) {

                if (callback) {
                    callback(e);
                } else {
                    $("#btnBoardControlBind").click();
                }
                e.preventDefault();
                return false;
            }

            return true;
        }
