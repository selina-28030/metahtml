	<script type="text/javascript">
	jQuery11(function(){
	
		var slider = jQuery11('#photoSlide2137').bxSlider({
		    auto: true,
			autoHover: true,
		    pause: 2000,
		    speed: 500,
            hideControlOnEnd: true,
            useCSS: false,
            slideWidth: 200,
		    minSlides: 1,
		    maxSlides: 5,
		    moveSlides: 1,
		    slideMargin: 20,
		    onSliderLoad: function (currentIndex){
		        jQuery11(".hcon2137").css("height", "inherit");
		    }
		});
		
        jQuery11('#boxCon2137 .bx-controls-direction a').click(function(e){
            var i = $(this).index();
            slider.goToSlide(i);
            slider.stopAuto();
            restart=setTimeout(function(){
                slider.startAuto();
                },500);
            return false;
        });
	});
	</script>
	
