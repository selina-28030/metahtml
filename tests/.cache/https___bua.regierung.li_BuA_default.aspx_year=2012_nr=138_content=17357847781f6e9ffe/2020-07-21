<script type="text/javascript">
    var nr = 138, year = 2012, suffix = '';
    var contentId = '1735784778'
    var printDialogOnlyFullDocument = false;
    var noMarginsForCenterContent = true;

    var buacontentscroller = $('#buacontent').parent();
    var contentInit = true;
    var scrollContentTimeout;
    var scrollToFunc = undefined;

    $.each($('#ddEntries').find('option'), function(ind, obj){
        obj = $(obj);
        var value = obj.val();
        var index = value.indexOf(';');
        var cid = value.substring(0, index);
        value = value.substring(index + 1);
        obj.val(value);
        obj.attr('id', 'printContentSel-' + cid);
    });

    function checkContainer(){
        checkContainerFunc();  
            
        ScrollChecks(false);
        scrollToFunc = function(){
            var enableScrollChecks = true;
            var content = $('#content-' + contentId);
            if ($('#buacontent').length > 0 && content.length > 0) {
                if(content.prev().length == 0){
                    buacontentscroller[0].scrollTop =0;
                }else{
                    buacontentscroller[0].scrollTop = content.position().top;

                    if(buacontentscroller[0].scrollHeight - buacontentscroller.scrollTop() === buacontentscroller.outerHeight() ) {
                        enableScrollChecks = false;
                        var contents = $('.contentDiv');
                        if($(contents[contents.length-1]).text() == ''){
                            getContent(true, false);
                        } else {
                            enableScrollChecks = true;
                        }
                    }
                }
            }
            if(enableScrollChecks){
                scrollToFunc = undefined;
            }
            return enableScrollChecks;
        };
        if(scrollToFunc.call()){
            initTocInfos();
            ScrollChecks(true);
        }
    }

    addLayoutCompletedEventListener(function (e) {
        setContentPadding();

        if (!resize) { mainLeftDiv.attr('ignoreTouchClose', '1'); }

        

        if(!resize){
            
            setCookie(nr, year, suffix);
            
            checkContainer();
        } else{
            ScrollChecks(false);
            initTocInfos();
            content2menuFunc.call();
            ScrollChecks(true);
        }

        $('#pnlPrint').width(viewportWidth);
        $('#pnlPrint').height(viewportHeight);

        $('#tocMenuDiv').scrollTo($('.tocItemSelected')[0]);
    });

    var ddS = document.getElementById('ddStart');
    var ddE = document.getElementById('ddEnd');

    function printDialog(open){
        if(!printDialogOnlyFullDocument){
            if(open){
                $('#pnlPrint').show();
                $(document).on("keyup.printdialog", function (e) {
                    if (e.keyCode == 27) {
                        printDialog(false);
                    }
                });
            }else{
                $('#pnlPrint').hide();
                $(document).off("keyup.printdialog");
            }
        } else{
            openPDFFenster('');
        }
    }

    function selectPages(obj) {
        var n = obj[obj.selectedIndex].value.split(';');
        setSelectedValue(ddS, n[0]);
        setSelectedValue(ddE, n[1]);
        $('#printPages').prop('checked', true);
    }

    function setSelectedValue(selectObj, valueToSet) {
        for (var i = 0; i < selectObj.options.length; i++) {
            if (selectObj.options[i].text == valueToSet) {
                selectObj.options[i].selected = true;
                return;
            }
        }
    }

    function openPDF() {
        var link = '';

        if(!$('#printAll').is(':checked')){
            link = '&ps=' + ddS[ddS.selectedIndex].value + '&pe=' + ddE[ddE.selectedIndex].value;
            if ($('#printFrontpage').is(':checked')) {
                link += '&title=1';
            }
        }
        openPDFFenster(link);
        printDialog(false);
    }

    function openPDFFenster(link) {
        var DruckLink = 'pdfshow.aspx?nr=138&year=2012' + link;
        var DruckTitel = "_blank";
        var DruckOptionen = "width=600,height=400,toolbar=no,location=no,directories=no,status=no,menubar=no,scrollbars=yes,resizable=yes,left=100,top=100";
        var DruckFenster = window.open(DruckLink, DruckTitel, DruckOptionen);
        DruckFenster.focus();
    }

    function checkContent(){
        if(checkScroll){
            var tmp = buacontentscroller[0].scrollHeight - buacontentscroller.scrollTop();
            var h = buacontentscroller.outerHeight();
            if (tmp >= h - 1 && tmp <= h + 1) {
                getContent(true, false);
            } else if(buacontentscroller.scrollTop() == 0){
                getContent(false, false);
            }
        }
    }

    function checkContainerFunc() {
        if ($('#buacontent').outerHeight(true) < $('#buacontent').parent().height()) {
            getContent(true, true);
        } else {
            buacontentscroller.on('scroll.content2menu', content2menuFunc);
            buacontentscroller.on('scroll.content', checkContent);
            ScrollChecks(true);
        }
    }

    var loaderTimeout;

    function getContent(next, checkContainer) {
        var contents = $('.contentDiv');
        if (contents.length > 0) {
            var content = $(contents[next ? contents.length - 1 : 0]);
            if (content.text() == '') {
                ScrollChecks(false);
                loaderTimeout = setTimeout(function(){
                    content.html('<div class="loader"></div>');
                    buacontentscroller[0].scrollTop = buacontentscroller[0].scrollHeight - buacontentscroller.outerHeight();
                    if(next){
                        buacontentscroller[0].scrollTop = buacontentscroller[0].scrollHeight - buacontentscroller.outerHeight();
                    } else{
                        buacontentscroller[0].scrollTop = 0;
                    }
                }, 150);
                
                setTimeout(function(){
                    loadBuaContent(checkContainer, content.prop('id').replace('content-', ''), !next, next, content)}, 0);
            }
        }
    }

    function loadBuaContent(checkContainer, content, addprev, addnext, div) {
        $.ajax({
            type: "POST",
            async: true,
            url: "Default.aspx/getBua",
            data: "{ content: '"+content+"', nr: '"+nr+"', year: '"+year+"', suffix: '"+suffix+"', next: "+addnext+", prev: "+addprev+ " }",
            contentType: "application/json; charset=utf-8",
            dataType: "json",
            error: function(){
                clearTimeout(loaderTimeout);
                ScrollChecks(true);
            },
            success: function (data) {
                ScrollChecks(false);
                clearTimeout(loaderTimeout);
                data = JSON.parse(data.d);

                var dom = undefined;
                if(addprev){
                    dom = $('#'+  $(data[4]).last().attr('id'));
                } else if(addnext){
                    dom = $('#'+  $(data[4]).first().attr('id'));
                }
                if(dom.length > 0){
                    dom.replaceWith(data[4]);
                }

                try{
                    if(HighlighttextFunction != undefined){
                        HighlighttextFunction.call();
                    }
                }catch(Exception){

                }

                setPages();

                createFootnotes();

                try{
                    LPDiscussionFunc.call();
                }catch(Exception){

                }

                var enableScrollChecks = true;

                if(scrollToFunc != undefined){
                    //buacontentscroller.scrollPos(div[0]);
                    enableScrollChecks = scrollToFunc.call();
                    initTocInfos();
                } else {
                    if(buacontentscroller.scrollTop() == 0){
                        buacontentscroller[0].scrollTop = 50;
                    }
                    initTocInfos();
                }

                if (checkContainer) {
                    checkContainerFunc();
                } else if(enableScrollChecks){
                    
                    ScrollChecks(true);
                    setMenuItem = true;
                }
            }
        });
    }

    function loadContent(content){
        ScrollChecks(false);
        contentId = content;
        loaderTimeout = setTimeout(function () {
            $('#buacontent').html('<div class="loader"></div>');
        }, 150);

        $.ajax({
            type: "POST",
            async: true,
            url: "Default.aspx/getBua2",
            data: "{ content: '" + content + "', nr: '" + nr + "', year: '" + year + "', suffix: '" + suffix + "'}",
            contentType: "application/json; charset=utf-8",
            dataType: "json",
            error: function () {
                clearTimeout(loaderTimeout);
                ScrollChecks(true);
            },
            success: function (data) {
                ScrollChecks(false);

                clearTimeout(loaderTimeout);
                data = JSON.parse(data.d);
                $('#buacontent').html(data[4]);
                try {
                    if (HighlighttextFunction != undefined) {
                        HighlighttextFunction.call();
                    }
                } catch (Exception) {

                }
                setPages();
                createFootnotes();

                try {
                    LPDiscussionFunc.call();
                } catch (Exception) {

                }

                if (layout == layout3) {
                    hideMenu($('#mainLeftDiv'));
                }

                checkContainer();
                content2menuFunc();
            }
        });
    }

    

    $(document).ready(function () {
        if ((new MobileDetect(window.navigator.userAgent)).mobile() != undefined) {
            $('#buacontent').find(':bold').addClass('boldMobile')
        } else {
            $('#buacontent').find(':bold').addClass('bold')
        }
        $('#buacontent').find(':italic').addClass('italic');

        createFootnotes();
    });

    function createFootnotes(){
        var showOptions = {
            solo: false,
        };
        var hideOptions = {
            fixed: true
        };
        if ($.mobile.support.touch) {
            showOptions["event"] = 'click';
            hideOptions["fixed"] = true;
            hideOptions["event"] = 'unfocus';
        }

        $.each($("*[class='fussnoteLink notInit']"), function (index, obj) {
            obj = $(obj);
            obj.removeClass('notInit');
            var a = obj.parent();
            var tds = $('#' + a.attr('id').replace('fr', 'fn')).find('td');
            if(tds.length == 2){
                var textStr = $(tds[1]).text();

                $(obj).qtip({
                    content: {
                        text: textStr
                    },
                    position: {
                        my: 'bottom center',  // Position my top left...
                        at: 'top center', // at the bottom right of...
                        target: $(obj),
                        viewport: $('#chdecml'),
                        adjust: {
                            resize: true,
                            method: 'shift none',
                        },
                        effect: false,
                    },
                    style: {
                        classes: 'qtip-light qtip-shadow qtip-rounded',
                    },
                    show: showOptions,
                    hide: hideOptions
                });

                if ($.mobile.support.touch == true) {
                    $(obj).on('click', function () {
                        gotoFn($(this).attr('name').replace('fussnoteRef', 'fussnoteDef'));
                    });
                    a.attr('href', '#');
                }
            }
        });
    }

    function hideShowPages(hide) {
        if (hide) {
            $('.kopfzeilenseite').hide()
        }
        else {
            $('.kopfzeilenseite').show()
        }
    }
</script>
