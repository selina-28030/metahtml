<script type="text/javascript">
    var pageNavLink = 'default.aspx?year=2012&nr=138';

    function showHeaderNav() {
        var animations = [];
        var headerH = $('#navHeader').outerHeight(), h = $('#mainDiv').outerHeight(), header = $('#header2').height();

        var changeSelected = false;

        if ($('#navHeader').is(':visible')) {
            changeSelected = true;

            animations.push(animateHide($('#navHeader'), 'up', animationLength));
            h += headerH;
            header -= headerH;
        } else {
            $('.headerRpCellAppSelected2').addClass('headerRpCellAppSelected').removeClass('headerRpCellAppSelected2');

            animations.push(animateShow($('#navHeader'), 'up', animationLength));
            h -= headerH;
            header += headerH;
        }
        animations.push(animateHeightResize($('#mainDiv'), h, animationLength));

        $.when.apply(null, animations).done(function () {
            if (changeSelected) {
                $('.headerRpCellAppSelected').addClass('headerRpCellAppSelected2').removeClass('headerRpCellAppSelected');
            }
            adjustLayout();
        });
    }


    function togglePages() {
        if ($('#chbSeitennummer').is(':checked')) {
            $('#chbSeitennummer').prop("checked", false);
            updateNavUrls(false);
            setPageDivVisibility(false);
        } else {
            $('#chbSeitennummer').prop("checked", true);
            updateNavUrls(true);
            setPageDivVisibility(true);
        }
    }

    function setPages() {
        if ($('#chbSeitennummer').is(':checked')) {
            updateNavUrls(true);
            setPageDivVisibility(true);
        } else {
            updateNavUrls(false);
            setPageDivVisibility(false);
        }
    }

    function updateNavUrls(checked) {

    }

    function setPageDivVisibility(visible) {
        if (visible) {
            $('.kopfzeilenseite').show();
        } else {
            $('.kopfzeilenseite').hide();
        }
    }

    $(document).ready(function () {
        if ($('#chbSeitennummer').is(':checked')) {
            updateNavUrls(true);
            setPageDivVisibility(true);
        } else {
            setPageDivVisibility(false);
        }

        addLayoutCompletedEventListener(function (e) {
            var w = menuSize;



            if (layout == layout1) {
                $('#tdRight').width(w - $('#tdRight').next().width());
            } else {
                $('#tdRight').hide();
            }

            
            var w = menuSize + $('#logoExt').width();
            if (layout == layout3 || layout == layout4) {
                w = 176;
            }
            $('.headerEInfoCol1').outerWidth(w);

            if (layout == layout1) {
                $('#tdRight').width(w - $('#tdRight').next().width());
            } else {
                $('#tdRight').hide();
            }

            
            var leftValue = 0;

            var moveToSmallContainer = false;
            if (viewportWidth < 1050 && viewportWidth >= 800) {
                $('#pageNavLp').css('margin-left', '20px');
                //$($('#pageNavLp .headerESeperator')[0]).hide();
                $('#pageNavLp').css({
                    left: 0
                });
            } else if (viewportWidth < 800) {
                $('#pageNavLp').css('margin-left', '20px');
                moveToSmallContainer = true;
                //$($('#pageNavLp .headerESeperator')[0]).hide();
            }
            else {
                $('#pageNavLp').css('margin-left', '');
                $('#pageNavSeitenNr').addClass('pageNavSeitenNrRight');
                leftValue = $('#pageNavLp').parent().width() / 2 - $('#pageNavLp').width() / 2
                $($('#pageNavLp .headerESeperator')[0]).show();
                $('#pageNavLp').css({
                    left: leftValue - 50
                });
            }


            if (moveToSmallContainer) {
                $('#pageNavSeitenNr').removeClass('pageNavSeitenNrRight');
                if ($('#pageNavSeitenNr').parents('#headerPageNavSmallLayoutContent').length == 0) {
                    $('#pageNavSeitenNr').appendTo('#headerPageNavSmallLayoutContent');
                    $('.headerPageNavSmallLayout').show();
                }

                //if ($('#pageNavLp .headerESeperator')[0].clientWidth < 2) {
                //    var p = parseInt($('#pageNavLp td').not('.headerESeperator').css('padding-left')) - 1;
                //    while ($('#pageNavLp .headerESeperator')[0].clientWidth < 2 && p >= 5) {
                //        $('#pageNavLp td').not('.headerESeperator').css('padding', '0 ' + p + 'px');
                //        p -= 1;
                //    }
                //} else {
                //    $('#pageNavLp td').not('.headerESeperator').css('padding', '');
                //}
            } else {
                $('#pageNavSeitenNr').addClass('pageNavSeitenNrRight');
                if ($('#pageNavSeitenNr').parents('#headerPageNavSmallLayoutContent').length > 0) {
                    $('#pageNavSeitenNr').appendTo($('#pageNavLp').parent());
                    $('.headerPageNavSmallLayout').hide();
                }
            }
            $('#pageNavSeitenNr').css({
                left: leftValue + $('#pageNavLp').outerWidth(true) - 30,
                top: '3px'
            });
            
        });
    });
</script>

